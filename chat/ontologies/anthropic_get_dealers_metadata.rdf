<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Vendor/Dealer Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling vendor and dealer management systems, including channel partners, business relationships, field metadata, user profiles, permissions, and operational workflows. This ontology captures the complete semantic structure of dealer onboarding, status management, business categorization, and multi-module integration.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Vendor">
    <rdfs:label>Vendor</rdfs:label>
    <rdfs:comment>A business entity that provides goods or services. Also referred to as a Dealer in the system. Represents channel partners, referral partners, strategic partners, consultants, system integrators, and architects.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Dealer">
    <rdfs:label>Dealer</rdfs:label>
    <rdfs:comment>A specialized vendor entity representing business partners who distribute or sell products. Synonymous with Vendor in this system context.</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://example.org/ontology#Vendor"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ChannelPartner">
    <rdfs:label>Channel Partner</rdfs:label>
    <rdfs:comment>A type of dealer that acts as an intermediary in the distribution channel, actively selling and promoting products.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReferralPartner">
    <rdfs:label>Referral Partner</rdfs:label>
    <rdfs:comment>A type of dealer that refers potential customers or opportunities without directly selling products.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StrategicPartner">
    <rdfs:label>Strategic Partner</rdfs:label>
    <rdfs:comment>A high-level dealer with strategic business alignment and long-term collaboration agreements.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Consultant">
    <rdfs:label>Consultant</rdfs:label>
    <rdfs:comment>A dealer type that provides expert advice and consulting services related to products or solutions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemIntegrator">
    <rdfs:label>System Integrator</rdfs:label>
    <rdfs:comment>A dealer specializing in integrating various systems, technologies, and solutions for clients.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Architect">
    <rdfs:label>Architect</rdfs:label>
    <rdfs:comment>A dealer type representing architectural firms or professionals who specify and design systems.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Dealer"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual person associated with business entities, representing points of contact for communication and relationship management.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Company">
    <rdfs:label>Company</rdfs:label>
    <rdfs:comment>A business organization or corporate entity. Referred to as Accounts in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>A business account representing a company or organization in the CRM system.</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://example.org/ontology#Company"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A system user who interacts with the CRM platform, including administrators, standard users, and dealers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Owner">
    <rdfs:label>Owner</rdfs:label>
    <rdfs:comment>A user who has ownership responsibility for records, typically managing and overseeing specific vendors or dealers.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Campaign">
    <rdfs:label>Campaign</rdfs:label>
    <rdfs:comment>A marketing or business campaign that can be associated with dealers for tracking engagement and performance.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Field and Metadata Classes            -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>A data field or attribute in the system that captures specific information about entities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextField">
    <rdfs:label>Text Field</rdfs:label>
    <rdfs:comment>A field that stores textual data such as names, descriptions, and addresses.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PhoneField">
    <rdfs:label>Phone Field</rdfs:label>
    <rdfs:comment>A field specifically for storing phone numbers with appropriate formatting.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailField">
    <rdfs:label>Email Field</rdfs:label>
    <rdfs:comment>A field for storing email addresses with validation constraints.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebsiteField">
    <rdfs:label>Website Field</rdfs:label>
    <rdfs:comment>A field for storing website URLs and web addresses.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateField">
    <rdfs:label>Date Field</rdfs:label>
    <rdfs:comment>A field for storing date values without time components.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeField">
    <rdfs:label>DateTime Field</rdfs:label>
    <rdfs:comment>A field for storing date and time values with full timestamp information.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field with predefined options from which users can select a single value.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MultiselectPicklistField">
    <rdfs:label>Multiselect Picklist Field</rdfs:label>
    <rdfs:comment>A field allowing selection of multiple values from predefined options.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that creates a relationship to another module or entity through reference.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanField">
    <rdfs:label>Boolean Field</rdfs:label>
    <rdfs:comment>A field storing true/false or yes/no values.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FileUploadField">
    <rdfs:label>File Upload Field</rdfs:label>
    <rdfs:comment>A field that allows uploading and storing file attachments.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaField">
    <rdfs:label>Text Area Field</rdfs:label>
    <rdfs:comment>A large text field for storing extended descriptions and multi-line content.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
</owl:Class>

<!-- ===================================== -->
<!-- Status and Lifecycle Classes          -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#DealerStatus">
    <rdfs:label>Dealer Status</rdfs:label>
    <rdfs:comment>The current stage or state of a dealer in the onboarding and management lifecycle.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ActiveStatus">
    <rdfs:label>Active Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer is fully operational and actively engaged.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#InactiveStatus">
    <rdfs:label>Inactive Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer is not currently active or operational.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AgreementInReviewStatus">
    <rdfs:label>Agreement In Review Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer agreement is under review and evaluation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AgreementIssuedStatus">
    <rdfs:label>Agreement Issued Status</rdfs:label>
    <rdfs:comment>Status indicating the agreement has been issued to the dealer for signature.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrainingPendingStatus">
    <rdfs:label>Training Pending Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer is awaiting training before full activation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RejectedStatus">
    <rdfs:label>Rejected Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer application or partnership has been rejected.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NonResponsiveStatus">
    <rdfs:label>Non Responsive Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer is not responding to communications.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealerStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>The system-level status of a record indicating its availability and state.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableRecordStatus">
    <rdfs:label>Available Record Status</rdfs:label>
    <rdfs:comment>Status indicating the record is available and accessible.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashRecordStatus">
    <rdfs:label>Trash Record Status</rdfs:label>
    <rdfs:comment>Status indicating the record has been moved to trash and is pending deletion.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftRecordStatus">
    <rdfs:label>Draft Record Status</rdfs:label>
    <rdfs:comment>Status indicating the record is in draft state and not yet finalized.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileScoreStatus">
    <rdfs:label>Profile Score Status</rdfs:label>
    <rdfs:comment>The evaluation status of a dealer profile based on screening and qualification criteria.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApprovedProfileStatus">
    <rdfs:label>Approved Profile Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer profile has been approved after evaluation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProfileScoreStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RejectedProfileStatus">
    <rdfs:label>Rejected Profile Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer profile has been rejected after evaluation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProfileScoreStatus"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ExploratoryProfileStatus">
    <rdfs:label>Exploratory Profile Status</rdfs:label>
    <rdfs:comment>Status indicating the dealer profile is under exploratory evaluation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProfileScoreStatus"/>
</owl:Class>

