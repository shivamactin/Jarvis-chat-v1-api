</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PublicLimitedCompany">
    <rdfs:label>Public Limited Company</rdfs:label>
    <rdfs:comment>A business constitution where the company is publicly traded with limited liability.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessConstitution"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PrivateLimitedCompany">
    <rdfs:label>Private Limited Company</rdfs:label>
    <rdfs:comment>A business constitution where the company is privately held with limited liability.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessConstitution"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LimitedLiabilityPartnership">
    <rdfs:label>Limited Liability Partnership</rdfs:label>
    <rdfs:comment>A business constitution combining partnership flexibility with limited liability protection.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessConstitution"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PartnershipFirm">
    <rdfs:label>Partnership Firm</rdfs:label>
    <rdfs:comment>A business constitution where two or more individuals share ownership and management.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessConstitution"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProprietaryFirm">
    <rdfs:label>Proprietary Firm</rdfs:label>
    <rdfs:comment>A business constitution owned and operated by a single individual.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessConstitution"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessCategory">
