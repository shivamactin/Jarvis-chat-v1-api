</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BuildingManagementSystemIntegrator">
    <rdfs:label>Building Management System Integrator</rdfs:label>
    <rdfs:comment>Business category for companies specializing in BMS integration and management.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HVACContractor">
    <rdfs:label>HVAC Contractor</rdfs:label>
    <rdfs:comment>Business category for companies providing heating, ventilation, and air conditioning services.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ElectricalContractor">
    <rdfs:label>Electrical Contractor</rdfs:label>
    <rdfs:comment>Business category for companies providing electrical installation and maintenance services.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FacilityManagementCompany">
    <rdfs:label>Facility Management Company</rdfs:label>
    <rdfs:comment>Business category for companies providing comprehensive facility management services.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RenewableEnergySolutionProvider">
    <rdfs:label>Renewable Energy Solution Provider</rdfs:label>
    <rdfs:comment>Business category for companies providing renewable energy systems and solutions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataCenterInfrastructureProvider">
    <rdfs:label>Data Center Infrastructure Provider</rdfs:label>
    <rdfs:comment>Business category for companies providing data center infrastructure and services.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessCategory"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerCategory">
