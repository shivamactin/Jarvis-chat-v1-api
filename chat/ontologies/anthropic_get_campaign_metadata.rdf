<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Campaign Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling campaign management fields, including field metadata, profiles, layouts, data types, operations, customization properties, and relationships within a CRM system context.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Classes: Field and Field Types  -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field within a campaign management system, containing metadata about its properties, behavior, and relationships.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is automatically managed by the system and cannot be modified by users.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A field created by users to extend the default data model with custom attributes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must be populated before a record can be saved.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that can be viewed but not edited by users.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Editable Field</rdfs:label>
    <rdfs:comment>A field that can be modified by users with appropriate permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VirtualField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Virtual Field</rdfs:label>
    <rdfs:comment>A field that is computed or derived rather than stored directly in the database.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DisplayField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Display Field</rdfs:label>
    <rdfs:comment>A field designated as the primary display identifier for records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Searchable Field</rdfs:label>
    <rdfs:comment>A field that can be used in search queries.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FilterableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Filterable Field</rdfs:label>
    <rdfs:comment>A field that can be used to filter records in views and reports.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SortableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Sortable Field</rdfs:label>
    <rdfs:comment>A field that can be used to sort records in ascending or descending order.</rdfs:comment>
</owl:Class>

