<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Custom Module Field Metadata Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology representing field metadata structures, including field types, properties, permissions, operations, and relationships within a CRM or custom module system. This ontology captures the complete semantic model of field definitions, user profiles, layout associations, and data management operations.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field within a module or form. Fields are the fundamental units of data capture and storage, each with specific properties, types, and behaviors.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system, typically non-customizable and essential for system operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field that can be customized, modified, or removed according to business requirements.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that can be viewed but not modified by users, typically containing system-generated or protected data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Editable Field</rdfs:label>
    <rdfs:comment>A field that users can modify according to their permissions and the field's configuration.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must contain a value before a record can be saved or submitted.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VirtualField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Virtual Field</rdfs:label>
    <rdfs:comment>A field that does not store data directly but is computed or derived from other fields or sources.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DisplayField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Display Field</rdfs:label>
    <rdfs:comment>A field designated as the primary display identifier for records in list views and references.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Field Type Classes                    -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#FieldType">
    <rdfs:label>Field Type</rdfs:label>
    <rdfs:comment>Represents the data type and behavior category of a field, determining how data is stored, validated, and displayed.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Text Field Type</rdfs:label>
    <rdfs:comment>A field type for storing textual data with configurable length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Picklist Field Type</rdfs:label>
    <rdfs:comment>A field type that presents users with a predefined list of values to choose from.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Lookup Field Type</rdfs:label>
    <rdfs:comment>A field type that creates relationships between records by referencing another module or entity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupFieldType"/>
    <rdfs:label>Owner Lookup Field Type</rdfs:label>
    <rdfs:comment>A specialized lookup field type for referencing users or teams who own or are responsible for records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>DateTime Field Type</rdfs:label>
    <rdfs:comment>A field type for storing date and time information with timezone considerations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>BigInt Field Type</rdfs:label>
    <rdfs:comment>A field type for storing large integer values, typically used for unique identifiers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileImageFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Profile Image Field Type</rdfs:label>
    <rdfs:comment>A field type for storing and displaying profile or record images.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Boolean Field Type</rdfs:label>
    <rdfs:comment>A field type for storing true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TagFieldType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FieldType"/>
    <rdfs:label>Tag Field Type</rdfs:label>
    <rdfs:comment>A field type for storing multiple tags or labels as an array of values.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Data Type Classes                     -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Represents the underlying data storage type for field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
