</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>Data type for owner reference relationships.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>Data type for temporal information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>BigInt Data Type</rdfs:label>
    <rdfs:comment>Data type for large integer values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileImageDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Profile Image Data Type</rdfs:label>
    <rdfs:comment>Data type for image file references.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>Data type for binary true/false values.</rdfs:comment>
</owl:Class>

