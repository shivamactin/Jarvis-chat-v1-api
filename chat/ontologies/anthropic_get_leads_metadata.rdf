<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>CRM Lead Management Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling lead management, customer relationship management, field metadata, user profiles, business processes, and data governance in a CRM system. This ontology captures entities, relationships, workflows, permissions, and data quality aspects derived from field metadata specifications.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===== Core Entity Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Entity">
    <rdfs:label>Entity</rdfs:label>
    <rdfs:comment>The top-level abstract class representing any identifiable entity in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessEntity">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Entity"/>
    <rdfs:label>Business Entity</rdfs:label>
    <rdfs:comment>Represents business-related entities such as leads, contacts, accounts, and deals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Lead">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Lead</rdfs:label>
    <rdfs:comment>A potential customer or business opportunity that has been identified and is being tracked in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual person associated with an account or business relationship.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>An organization or company that is a customer or potential customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Deal">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Deal</rdfs:label>
    <rdfs:comment>A sales opportunity or transaction being pursued with a lead or account.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Vendor">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Vendor</rdfs:label>
    <rdfs:comment>A supplier or dealer providing products or services.</rdfs:comment>
</owl:Class>

<!-- ===== Person and User Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Person">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Entity"/>
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>An individual human being with personal attributes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A person who has access to and uses the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LeadOwner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
