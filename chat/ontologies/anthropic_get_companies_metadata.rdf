<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>CRM Field Metadata Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology representing CRM system field metadata, including field definitions, properties, relationships, profiles, permissions, data types, and operational characteristics for account/company management systems.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field in a CRM system that captures specific information about entities such as companies, accounts, or contacts. Fields have various properties including data types, visibility settings, and operational constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is automatically managed by the system and typically cannot be modified by users. Examples include Created Time, Modified Time, and system-generated identifiers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field created to capture specific business information not covered by standard system fields. Custom fields can be tailored to organizational needs.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#SystemField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must be populated with a value before a record can be saved. System mandatory fields are required by the system architecture.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that can be viewed but not edited by users. Often used for system-calculated or audit fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VirtualField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Virtual Field</rdfs:label>
    <rdfs:comment>A field that does not store data directly but displays computed or aggregated information from other sources.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Defines the type of data that can be stored in a field, such as text, number, date, boolean, or complex types like lookup or picklist.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Text Data Type</rdfs:label>
    <rdfs:comment>A data type for storing textual information with specified maximum length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Numeric Data Type</rdfs:label>
    <rdfs:comment>A data type for storing numerical values, including integers and decimals with precision specifications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>A data type for storing temporal information including dates and times.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>A data type for storing true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Picklist Data Type</rdfs:label>
    <rdfs:comment>A data type that restricts values to a predefined list of options from which users can select.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Lookup Data Type</rdfs:label>
    <rdfs:comment>A data type that establishes relationships between records by referencing another module or entity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>A specialized lookup type that references user or ownership information for record management.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PhoneDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextDataType"/>
    <rdfs:label>Phone Data Type</rdfs:label>
    <rdfs:comment>A specialized text data type for storing telephone numbers with appropriate formatting.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebsiteDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextDataType"/>
    <rdfs:label>Website Data Type</rdfs:label>
    <rdfs:comment>A specialized text data type for storing URL/website addresses with validation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextDataType"/>
    <rdfs:label>Text Area Data Type</rdfs:label>
    <rdfs:comment>A data type for storing large blocks of text with multi-line support.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileImageDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Profile Image Data Type</rdfs:label>
    <rdfs:comment>A data type for storing image files associated with records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TerritoriesDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Territories Data Type</rdfs:label>
    <rdfs:comment>A specialized data type for managing geographical or organizational territory assignments.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PickListValue">
    <rdfs:label>Pick List Value</rdfs:label>
    <rdfs:comment>Represents an individual option within a picklist field, containing display value, actual value, and metadata.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UsedPickListValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Used Pick List Value</rdfs:label>
    <rdfs:comment>A picklist value that is actively used in the system and available for selection.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UnusedPickListValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Unused Pick List Value</rdfs:label>
    <rdfs:comment>A picklist value that exists in the system but is not currently active or available for selection.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#UsedPickListValue"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#GlobalPicklist">
    <rdfs:label>Global Picklist</rdfs:label>
    <rdfs:comment>A reusable picklist definition that can be shared across multiple fields and modules in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>Represents a user profile or role in the CRM system that determines access permissions and capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full system access and administrative privileges.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user access and typical operational permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A specialized profile for dealer users with specific business-related permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>Defines the level of access a profile has to a specific field or resource.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Permission level that allows both viewing and editing of field data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Only Permission</rdfs:label>
    <rdfs:comment>Permission level that allows only viewing of field data without editing capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HiddenPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Hidden Permission</rdfs:label>
    <rdfs:comment>Permission level that prevents viewing or accessing the field entirely.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OperationType">
    <rdfs:label>Operation Type</rdfs:label>
    <rdfs:comment>Defines the types of operations that can be performed on a field through different interfaces.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>Operation that allows creating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>Operation that allows updating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>Operation that allows creating records through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>Operation that allows updating records through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>Defines the contexts in which a field can be viewed or edited within the user interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DetailViewType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Detail View Type</rdfs:label>
    <rdfs:comment>View type for displaying field in the detailed record view.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditViewType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Edit View Type</rdfs:label>
    <rdfs:comment>View type for displaying field in the record editing interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateViewType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Create View Type</rdfs:label>
    <rdfs:comment>View type for displaying field in the record creation interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuickCreateViewType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Quick Create View Type</rdfs:label>
    <rdfs:comment>View type for displaying field in the quick record creation popup.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Defines the arrangement and organization of fields within a module or form.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>Standard Layout</rdfs:label>
    <rdfs:comment>The default layout configuration for a module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>Represents a functional area or entity type in the CRM system, such as Accounts, Contacts, or Deals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AccountModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>Account Module</rdfs:label>
    <rdfs:comment>Module for managing company and organizational account information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SAPCustomerModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>SAP Customer Module</rdfs:label>
    <rdfs:comment>Module for managing SAP customer code and integration data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>Represents a property or characteristic of a field that can be customized by administrators.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryProperty">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Mandatory Property</rdfs:label>
    <rdfs:comment>Property that controls whether a field is required.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UniqueProperty">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Unique Property</rdfs:label>
    <rdfs:comment>Property that enforces uniqueness of field values across records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RemovalProperty">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Removal Property</rdfs:label>
    <rdfs:comment>Property that controls whether a field can be removed from layouts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TooltipProperty">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Tooltip Property</rdfs:label>
    <rdfs:comment>Property that allows adding help text or tooltips to fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HistoryTrackingProperty">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>History Tracking Property</rdfs:label>
    <rdfs:comment>Property that enables tracking of field value changes over time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>Configuration for parsing and extracting field values from email content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Address">
    <rdfs:label>Address</rdfs:label>
    <rdfs:comment>Represents a physical or mailing address with components like street, city, state, and country.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BillingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Billing Address</rdfs:label>
    <rdfs:comment>Address used for billing and invoicing purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Industry">
    <rdfs:label>Industry</rdfs:label>
    <rdfs:comment>Represents a business sector or industry classification.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AgricultureIndustry">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Industry"/>
    <rdfs:label>Agriculture Industry</rdfs:label>
    <rdfs:comment>Industry sector related to agricultural equipment and services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ConstructionIndustry">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Industry"/>
    <rdfs:label>Construction Industry</rdfs:label>
    <rdfs:comment>Industry sector related to construction equipment and services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ElectricVehicleIndustry">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Industry"/>
    <rdfs:label>Electric Vehicle Industry</rdfs:label>
    <rdfs:comment>Industry sector related to electric vehicle manufacturing and services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UtilitiesIndustry">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Industry"/>
    <rdfs:label>Utilities Industry</rdfs:label>
    <rdfs:comment>Industry sector related to utility services and infrastructure.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TechnologyIndustry">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Industry"/>
    <rdfs:label>Technology Industry</rdfs:label>
    <rdfs:comment>Industry sector related to technology products and services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Product">
    <rdfs:label>Product</rdfs:label>
    <rdfs:comment>Represents a product or service offering in the business domain.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SoftwareProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Software Product</rdfs:label>
    <rdfs:comment>A software-based product or solution.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HardwareProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Hardware Product</rdfs:label>
    <rdfs:comment>A physical hardware product or equipment.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ServiceProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Service Product</rdfs:label>
    <rdfs:comment>A service-based offering or managed service.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Represents a user of the CRM system with specific roles and permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Owner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Owner</rdfs:label>
    <rdfs:comment>A user who owns or is responsible for specific records in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Represents the current state or status of a record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Status indicating the record is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>Status indicating the record has been deleted or moved to trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Status indicating the record is in draft state and not yet finalized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EnrichmentStatus">
    <rdfs:label>Enrichment Status</rdfs:label>
    <rdfs:comment>Represents the status of data enrichment for a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Territory">
    <rdfs:label>Territory</rdfs:label>
    <rdfs:comment>Represents a geographical or organizational territory for sales or service management.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tag">
    <rdfs:label>Tag</rdfs:label>
    <rdfs:comment>A label or keyword used to categorize and organize records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AssociationDetails">
    <rdfs:label>Association Details</rdfs:label>
    <rdfs:comment>Contains information about relationships and associations between fields and modules.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupFilter">
    <rdfs:label>Lookup Filter</rdfs:label>
    <rdfs:comment>Defines filtering criteria for lookup field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Blueprint">
    <rdfs:label>Blueprint</rdfs:label>
    <rdfs:comment>Represents a business process automation configuration in the CRM system.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties (Relationships)     -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasDataType">
    <rdfs:label>has data type</rdfs:label>
    <rdfs:comment>Relates a field to its data type specification.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#DataType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isDataTypeOf">
    <rdfs:label>is data type of</rdfs:label>
    <rdfs:comment>Inverse of hasDataType - relates a data type to fields that use it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasDataType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasProfile">
    <rdfs:label>has profile</rdfs:label>
    <rdfs:comment>Relates a field to profiles that have access to it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Profile"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasAccessToField">
    <rdfs:label>has access to field</rdfs:label>
    <rdfs:comment>Inverse of hasProfile - relates a profile to fields it can access.</rdfs:comment>
