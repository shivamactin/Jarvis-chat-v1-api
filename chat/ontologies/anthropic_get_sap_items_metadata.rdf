    <rdfs:comment>A field that stores true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Numeric Field</rdfs:label>
    <rdfs:comment>A field that stores numeric values including integers and decimals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TagField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Tag Field</rdfs:label>
    <rdfs:comment>A field that stores multiple tags or labels for categorization and organization.</rdfs:comment>
</owl:Class>

