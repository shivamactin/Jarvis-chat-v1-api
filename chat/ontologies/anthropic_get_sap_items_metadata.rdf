<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>SAP Item Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling SAP Item management system with fields, profiles, permissions, operations, layouts, and data governance structures. This ontology captures the complete metadata structure including field properties, user roles, access controls, data types, validation rules, and system behaviors.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Entity">
    <rdfs:label>Entity</rdfs:label>
    <rdfs:comment>The top-level abstract class representing any identifiable entity in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemEntity">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Entity"/>
    <rdfs:label>System Entity</rdfs:label>
    <rdfs:comment>An entity that is part of the system infrastructure, including modules, fields, and configurations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessEntity">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Entity"/>
    <rdfs:label>Business Entity</rdfs:label>
    <rdfs:comment>An entity representing business domain concepts such as items, records, and transactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SecurityEntity">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Entity"/>
    <rdfs:label>Security Entity</rdfs:label>
    <rdfs:comment>An entity related to security, access control, and permissions management.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Field and Data Structure Classes      -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SystemEntity"/>
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>A data field within a module that stores specific information with defined properties, constraints, and behaviors.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is automatically managed by the system and typically read-only or system-generated.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A field created by users or administrators to extend the default data model.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#IdentifierField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SystemField"/>
