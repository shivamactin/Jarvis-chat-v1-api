<!-- ===================================== -->
<!-- Property Chains and Complex Relationships -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasAccessThroughProfile">
    <rdfs:label>has access through profile</rdfs:label>
    <rdfs:comment>Relates a user to fields they can access through their profile.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#User"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <owl:ObjectProperty rdf:about="http://example.org/ontology#hasUserProfile"/>
        <owl:ObjectProperty rdf:about="http://example.org/ontology#hasAccessToField"/>
    </owl:propertyChainAxiom>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#fieldsInModuleLayout">
    <rdfs:label>fields in module layout</rdfs:label>
    <rdfs:comment>Relates a module to fields displayed in its layouts.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
</owl:ObjectProperty>

