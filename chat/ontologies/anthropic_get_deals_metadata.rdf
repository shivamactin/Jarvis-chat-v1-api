<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>CRM Deal Management Ontology</rdfs:label>
    <rdfs:comment>Comprehensive ontology modeling CRM deal/opportunity management system with fields, relationships, business processes, and organizational structures. Captures sales pipeline stages, customer relationships, product lines, forecasting, and approval workflows.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===== Core Business Entity Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Deal">
    <rdfs:label>Deal</rdfs:label>
    <rdfs:comment>A business opportunity or potential transaction representing a sales deal with customers. Also known as Potential or Opportunity in CRM systems.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>A business entity or organization that is a customer or prospect in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual person associated with an account or deal, representing decision makers, influencers, or stakeholders.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Lead">
    <rdfs:label>Lead</rdfs:label>
    <rdfs:comment>A potential customer or business opportunity that has not yet been qualified as a deal.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Campaign">
    <rdfs:label>Campaign</rdfs:label>
    <rdfs:comment>A marketing or sales campaign that generates leads and deals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Vendor">
    <rdfs:label>Vendor</rdfs:label>
    <rdfs:comment>A supplier, partner, or dealer organization that may be involved in deal execution.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Product">
    <rdfs:label>Product</rdfs:label>
    <rdfs:comment>A product or service offering that can be sold through deals.</rdfs:comment>
</owl:Class>

