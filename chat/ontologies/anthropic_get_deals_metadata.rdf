<!-- ===== ===== OBJECT PROPERTIES ===== ===== -->

<!-- Deal Relationships -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasOwner">
    <rdfs:label>has owner</rdfs:label>
    <rdfs:comment>Relates a deal to its responsible owner.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Deal"/>
    <rdfs:range rdf:resource="http://example.org/ontology#DealOwner"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#ownedBy">
    <rdfs:label>owned by</rdfs:label>
    <rdfs:comment>Inverse of hasOwner.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#DealOwner"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Deal"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasOwner"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#associatedWithAccount">
    <rdfs:label>associated with account</rdfs:label>
    <rdfs:comment>Relates a deal to the customer account.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Deal"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Account"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasDeals">
    <rdfs:label>has deals</rdfs:label>
    <rdfs:comment>Inverse relationship from account to deals.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Account"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Deal"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#associatedWithAccount"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasPrimaryContact">
    <rdfs:label>has primary contact</rdfs:label>
    <rdfs:comment>Relates a deal to its primary contact person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Deal"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isPrimaryContactFor">
    <rdfs:label>is primary contact for</rdfs:label>
    <rdfs:comment>Inverse of hasPrimaryContact.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Deal"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasPrimaryContact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasSecondaryContact">
    <rdfs:label>has secondary contact</rdfs:label>
    <rdfs:comment>Relates a deal to additional secondary contacts.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Deal"/>
    <rdfs:range rdf:resource="http://example.org/ontology#SecondaryContact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isSecondaryContactFor">
    <rdfs:label>is secondary contact for</rdfs:label>
    <rdfs:comment>Inverse of hasSecondaryContact.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#SecondaryContact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Deal"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasSecondaryContact"/>
</owl:ObjectProperty>

