<!-- ===== Sales Pipeline and Stage Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#SalesPipeline">
    <rdfs:label>Sales Pipeline</rdfs:label>
    <rdfs:comment>A structured sales process with defined stages for managing deals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealStage">
    <rdfs:label>Deal Stage</rdfs:label>
    <rdfs:comment>A specific phase in the sales pipeline representing progress toward closing a deal.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OpenStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealStage"/>
    <rdfs:label>Open Stage</rdfs:label>
    <rdfs:comment>Deal stage indicating the opportunity is still active and being pursued.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ClosedWonStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealStage"/>
    <rdfs:label>Closed Won Stage</rdfs:label>
    <rdfs:comment>Deal stage indicating successful closure and customer commitment.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ClosedLostStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DealStage"/>
    <rdfs:label>Closed Lost Stage</rdfs:label>
    <rdfs:comment>Deal stage indicating the opportunity was lost to competition or abandoned.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QualificationStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OpenStage"/>
    <rdfs:label>Qualification Stage</rdfs:label>
    <rdfs:comment>Initial stage where lead is qualified as a viable opportunity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NeedsAnalysisStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OpenStage"/>
    <rdfs:label>Needs Analysis Stage</rdfs:label>
    <rdfs:comment>Stage where customer requirements and needs are analyzed.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProposalStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OpenStage"/>
    <rdfs:label>Proposal Stage</rdfs:label>
    <rdfs:comment>Stage where formal proposal or quote is submitted to customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NegotiationStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OpenStage"/>
    <rdfs:label>Negotiation Stage</rdfs:label>
    <rdfs:comment>Stage where terms, pricing, and conditions are negotiated.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PilotStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OpenStage"/>
    <rdfs:label>Pilot Stage</rdfs:label>
    <rdfs:comment>Stage where pilot or proof-of-concept implementation is conducted.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProductionStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ClosedWonStage"/>
    <rdfs:label>Production Stage</rdfs:label>
    <rdfs:comment>Stage where solution is in full production deployment.</rdfs:comment>
</owl:Class>

<!-- ===== Stage Status Tracking ===== -->

<owl:Class rdf:about="http://example.org/ontology#StageStatus">
    <rdfs:label>Stage Status</rdfs:label>
    <rdfs:comment>Current execution status of a deal stage.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NotInitiatedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>Not Initiated Status</rdfs:label>
    <rdfs:comment>Stage has not yet been started.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OnTrackStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>On Track Status</rdfs:label>
    <rdfs:comment>Stage is progressing according to plan.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DelayedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>Delayed Status</rdfs:label>
    <rdfs:comment>Stage is behind schedule.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CompletedOnTimeStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>Completed On Time Status</rdfs:label>
    <rdfs:comment>Stage completed within expected timeframe.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CompletedWithDelayStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>Completed With Delay Status</rdfs:label>
    <rdfs:comment>Stage completed but behind schedule.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OnHoldStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>On Hold Status</rdfs:label>
    <rdfs:comment>Stage temporarily paused or suspended.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TerminatedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#StageStatus"/>
    <rdfs:label>Terminated Status</rdfs:label>
    <rdfs:comment>Stage permanently stopped or cancelled.</rdfs:comment>
</owl:Class>

