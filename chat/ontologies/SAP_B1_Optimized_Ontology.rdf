  <!-- ========================================
       OBJECT PROPERTIES: DOCUMENT FLOW CHAIN
       ======================================== -->
  
  <!-- Base Document Flow Property -->
  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#derivesFromDocument">
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:label xml:lang="en">derives from document</rdfs:label>
    <rdfs:comment xml:lang="en">Links a document to its source/predecessor document in the business process flow (transitive property for chain traversal)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#leadsToDocument">
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#derivesFromDocument"/>
    <rdfs:label xml:lang="en">leads to document</rdfs:label>
    <rdfs:comment xml:lang="en">Links a document to its successor document in the business process flow</rdfs:comment>
  </owl:ObjectProperty>

  <!-- EXPLICIT SALES FLOW: Order → Delivery → Invoice → Journal Entry -->
  
  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#salesOrderCreatesDelivery">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#ORDR"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#ODLN"/>
    <rdfs:label xml:lang="en">sales order creates delivery</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 1→2: Sales Order (ORDR) leads to Delivery Note (ODLN)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#deliveryDerivedFromSalesOrder">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#salesOrderCreatesDelivery"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#ODLN"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#ORDR"/>
    <rdfs:label xml:lang="en">delivery derived from sales order</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 2→1: Delivery Note (ODLN) references Sales Order (ORDR)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#deliveryCreatesInvoice">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#ODLN"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OINV"/>
    <rdfs:label xml:lang="en">delivery creates invoice</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 2→3: Delivery Note (ODLN) leads to AR Invoice (OINV)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#invoiceDerivedFromDelivery">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#deliveryCreatesInvoice"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OINV"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#ODLN"/>
    <rdfs:label xml:lang="en">invoice derived from delivery</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 3→2: AR Invoice (OINV) references Delivery Note (ODLN)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#invoiceGeneratesJournalEntry">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OINV"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OJDT"/>
    <rdfs:label xml:lang="en">invoice generates journal entry</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 3→4: AR Invoice (OINV) generates Journal Entry (OJDT) for financial posting</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#journalEntryGeneratedByInvoice">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#invoiceGeneratesJournalEntry"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OJDT"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OINV"/>
    <rdfs:label xml:lang="en">journal entry generated by invoice</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 4→3: Journal Entry (OJDT) was created by AR Invoice (OINV)</rdfs:comment>
  </owl:ObjectProperty>

  <!-- EXPLICIT PURCHASE FLOW: PO → Goods Receipt → AP Invoice → Journal Entry -->
  
  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#purchaseOrderCreatesGoodsReceipt">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OPOR"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OPDN"/>
    <rdfs:label xml:lang="en">purchase order creates goods receipt</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 1→2: Purchase Order (OPOR) leads to Goods Receipt (OPDN)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#goodsReceiptDerivedFromPurchaseOrder">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#purchaseOrderCreatesGoodsReceipt"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OPDN"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OPOR"/>
    <rdfs:label xml:lang="en">goods receipt derived from purchase order</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 2→1: Goods Receipt (OPDN) references Purchase Order (OPOR)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#goodsReceiptCreatesAPInvoice">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OPDN"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OPCH"/>
    <rdfs:label xml:lang="en">goods receipt creates AP invoice</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 2→3: Goods Receipt (OPDN) leads to AP Invoice (OPCH)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#apInvoiceDerivedFromGoodsReceipt">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#goodsReceiptCreatesAPInvoice"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OPCH"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OPDN"/>
    <rdfs:label xml:lang="en">AP invoice derived from goods receipt</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 3→2: AP Invoice (OPCH) references Goods Receipt (OPDN)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#apInvoiceGeneratesJournalEntry">
    <rdfs:subPropertyOf rdf:resource="http://example.org/sap-b1/ontology#leadsToDocument"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OPCH"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OJDT"/>
    <rdfs:label xml:lang="en">AP invoice generates journal entry</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 3→4: AP Invoice (OPCH) generates Journal Entry (OJDT)</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#journalEntryGeneratedByAPInvoice">
    <owl:inverseOf rdf:resource="http://example.org/sap-b1/ontology#apInvoiceGeneratesJournalEntry"/>
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#OJDT"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OPCH"/>
    <rdfs:label xml:lang="en">journal entry generated by AP invoice</rdfs:label>
    <rdfs:comment xml:lang="en">FLOW STEP 4→3: Journal Entry (OJDT) created by AP Invoice (OPCH)</rdfs:comment>
  </owl:ObjectProperty>

