       ======================================== -->
  
  <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#completeFlowFromOrderToAccount">
    <rdfs:domain rdf:resource="http://example.org/sap-b1/ontology#ORDR"/>
    <rdfs:range rdf:resource="http://example.org/sap-b1/ontology#OACT"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#salesOrderCreatesDelivery"/>
      <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#deliveryCreatesInvoice"/>
      <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#invoiceGeneratesJournalEntry"/>
      <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#hasJournalLine"/>
      <owl:ObjectProperty rdf:about="http://example.org/sap-b1/ontology#journalLinePostsToAccount"/>
    </owl:propertyChainAxiom>
    <rdfs:label xml:lang="en">complete flow from order to account</rdfs:label>
    <rdfs:comment xml:lang="en">COMPLETE CHAIN: Sales Order → Delivery → Invoice → Journal Entry → Journal Line → G/L Account</rdfs:comment>
  </owl:ObjectProperty>

