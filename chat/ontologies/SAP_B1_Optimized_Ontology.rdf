<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:sap="http://example.org/sap-b1/ontology#"
         xml:base="http://example.org/sap-b1/ontology#">

  <owl:Ontology rdf:about="http://example.org/sap-b1/ontology#">
    <rdfs:label xml:lang="en">SAP Business One Ontology</rdfs:label>
    <rdfs:comment xml:lang="en">Comprehensive ontology for SAP Business One with explicit document flows, business logic, and inference-friendly relationships for chatbot integration</rdfs:comment>
    <owl:versionInfo>2.0</owl:versionInfo>
  </owl:Ontology>

  <!-- ========================================
       TOP-LEVEL ABSTRACT CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#SAPEntity">
    <rdfs:label xml:lang="en">SAP Entity</rdfs:label>
    <rdfs:comment xml:lang="en">Abstract top-level class for all SAP Business One entities including documents, master data, and transactions</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#Document">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SAPEntity"/>
    <rdfs:label xml:lang="en">Business Document</rdfs:label>
    <rdfs:comment xml:lang="en">A business document in SAP Business One such as orders, invoices, deliveries, payments, etc.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#MasterData">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SAPEntity"/>
    <rdfs:label xml:lang="en">Master Data</rdfs:label>
    <rdfs:comment xml:lang="en">Master data entities such as business partners, items, accounts that are referenced by transactional documents</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#TransactionalData">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SAPEntity"/>
    <rdfs:label xml:lang="en">Transactional Data</rdfs:label>
    <rdfs:comment xml:lang="en">Transactional data such as journal entries, payments that record business events</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#HeaderDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Header Document</rdfs:label>
    <rdfs:comment xml:lang="en">Document header containing summary information and linking to detail lines</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#DetailLine">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Detail Line</rdfs:label>
    <rdfs:comment xml:lang="en">Individual line item within a document containing item-level details</rdfs:comment>
  </owl:Class>

  <!-- ========================================
       MODULE CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#SalesDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Sales Document</rdfs:label>
    <rdfs:comment xml:lang="en">Sales-related documents including orders, deliveries, invoices, returns, and credit memos</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#PurchaseDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Purchase Document</rdfs:label>
    <rdfs:comment xml:lang="en">Purchase-related documents including purchase orders, goods receipts, AP invoices, and credit memos</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#FinancialDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Financial Document</rdfs:label>
    <rdfs:comment xml:lang="en">Financial documents including journal entries, payments that affect the general ledger</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#InventoryDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Inventory Document</rdfs:label>
    <rdfs:comment xml:lang="en">Inventory movement documents including goods receipts, goods issues, and inventory transfers</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ProductionDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#Document"/>
    <rdfs:label xml:lang="en">Production Document</rdfs:label>
    <rdfs:comment xml:lang="en">Production and manufacturing documents including BOMs and production orders</rdfs:comment>
  </owl:Class>

  <!-- ========================================
       SALES CYCLE CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ORDR">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">ORDR (Sales Order - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Sales Order Header: Customer request for goods or services, starting point of sales process. Leads to Delivery (ODLN) and Invoice (OINV).</rdfs:comment>
    <sap:tableCode>ORDR</sap:tableCode>
    <sap:businessProcess>Sales Order</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#RDR1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">RDR1 (Sales Order - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Sales Order Line Items: Individual items, quantities, and prices within a sales order</rdfs:comment>
    <sap:tableCode>RDR1</sap:tableCode>
    <sap:businessProcess>Sales Order Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ODLN">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">ODLN (Delivery Note - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Delivery Note Header: Records goods delivered to customer. Derived from Sales Order (ORDR), leads to Invoice (OINV).</rdfs:comment>
    <sap:tableCode>ODLN</sap:tableCode>
    <sap:businessProcess>Delivery Note</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#DLN1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">DLN1 (Delivery Note - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Delivery Note Line Items: Specific items and quantities delivered</rdfs:comment>
    <sap:tableCode>DLN1</sap:tableCode>
    <sap:businessProcess>Delivery Note Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OINV">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OINV (AR Invoice - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Accounts Receivable Invoice Header: Bills customer for goods/services. Derived from Delivery (ODLN), generates Journal Entry (OJDT).</rdfs:comment>
    <sap:tableCode>OINV</sap:tableCode>
    <sap:businessProcess>AR Invoice</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#INV1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">INV1 (AR Invoice - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">AR Invoice Line Items: Billed items with amounts and tax details</rdfs:comment>
    <sap:tableCode>INV1</sap:tableCode>
    <sap:businessProcess>AR Invoice Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ORIN">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">ORIN (AR Credit Memo - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">AR Credit Memo Header: Reduces customer's outstanding balance, typically for returns or corrections</rdfs:comment>
    <sap:tableCode>ORIN</sap:tableCode>
    <sap:businessProcess>AR Credit Memo</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#RIN1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#SalesDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">RIN1 (AR Credit Memo - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">AR Credit Memo Line Items</rdfs:comment>
    <sap:tableCode>RIN1</sap:tableCode>
    <sap:businessProcess>AR Credit Memo Detail</sap:businessProcess>
  </owl:Class>

  <!-- ========================================
       PURCHASE CYCLE CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OPOR">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OPOR (Purchase Order - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Purchase Order Header: Document sent to vendor specifying items, quantities, and prices. Leads to Goods Receipt (OPDN) and AP Invoice (OPCH).</rdfs:comment>
    <sap:tableCode>OPOR</sap:tableCode>
    <sap:businessProcess>Purchase Order</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#POR1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">POR1 (Purchase Order - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Purchase Order Line Items: Items being purchased with quantities and prices</rdfs:comment>
    <sap:tableCode>POR1</sap:tableCode>
    <sap:businessProcess>Purchase Order Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OPDN">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OPDN (Goods Receipt PO - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Goods Receipt Header: Records physical receipt of goods from vendor. Derived from Purchase Order (OPOR), leads to AP Invoice (OPCH).</rdfs:comment>
    <sap:tableCode>OPDN</sap:tableCode>
    <sap:businessProcess>Goods Receipt PO</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#PDN1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">PDN1 (Goods Receipt PO - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Goods Receipt Line Items: Items received from vendor</rdfs:comment>
    <sap:tableCode>PDN1</sap:tableCode>
    <sap:businessProcess>Goods Receipt PO Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OPCH">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OPCH (AP Invoice - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Accounts Payable Invoice Header: Records liability to vendor. Derived from Goods Receipt (OPDN), generates Journal Entry (OJDT).</rdfs:comment>
    <sap:tableCode>OPCH</sap:tableCode>
    <sap:businessProcess>AP Invoice</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#PCH1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">PCH1 (AP Invoice - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">AP Invoice Line Items: Invoiced items with amounts</rdfs:comment>
    <sap:tableCode>PCH1</sap:tableCode>
    <sap:businessProcess>AP Invoice Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ORPC">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">ORPC (AP Credit Memo - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">AP Credit Memo Header: Corrects or reverses AP transaction, typically for returns to vendor</rdfs:comment>
    <sap:tableCode>ORPC</sap:tableCode>
    <sap:businessProcess>AP Credit Memo</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#RPC1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#PurchaseDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">RPC1 (AP Credit Memo - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">AP Credit Memo Line Items</rdfs:comment>
    <sap:tableCode>RPC1</sap:tableCode>
    <sap:businessProcess>AP Credit Memo Detail</sap:businessProcess>
  </owl:Class>

  <!-- ========================================
       FINANCIAL CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OJDT">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#FinancialDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OJDT (Journal Entry - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Journal Entry Header: Financial postings to general ledger. Generated by business documents (invoices, payments) or created manually. Contains batch information and links to JDT1 lines.</rdfs:comment>
    <sap:tableCode>OJDT</sap:tableCode>
    <sap:businessProcess>Journal Entry</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#JDT1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#FinancialDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">JDT1 (Journal Entry - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Journal Entry Lines: Individual G/L account postings with debit and credit amounts. Each line posts to a specific account (OACT) in the chart of accounts.</rdfs:comment>
    <sap:tableCode>JDT1</sap:tableCode>
    <sap:businessProcess>Journal Entry Detail</sap:businessProcess>
    <sap:documentFlowPosition rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</sap:documentFlowPosition>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OACT">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#MasterData"/>
    <rdfs:label xml:lang="en">OACT (Chart of Accounts)</rdfs:label>
    <rdfs:comment xml:lang="en">Chart of Accounts: Hierarchical list of all G/L accounts. Journal entry lines (JDT1) post to these accounts. Contains account codes, names, and hierarchy.</rdfs:comment>
    <sap:tableCode>OACT</sap:tableCode>
    <sap:businessProcess>G/L Account Master</sap:businessProcess>
  </owl:Class>

  <!-- ========================================
       MASTER DATA CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OCRD">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#MasterData"/>
    <rdfs:label xml:lang="en">OCRD (Business Partner Master)</rdfs:label>
    <rdfs:comment xml:lang="en">Business Partner Master: Central record for customers, vendors, and leads. Referenced by all sales and purchase documents via CardCode field.</rdfs:comment>
    <sap:tableCode>OCRD</sap:tableCode>
    <sap:businessProcess>Business Partner</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OITM">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#MasterData"/>
    <rdfs:label xml:lang="en">OITM (Item Master)</rdfs:label>
    <rdfs:comment xml:lang="en">Item Master: Master data for products and services. Referenced by document lines via ItemCode field. Contains pricing, inventory, and product details.</rdfs:comment>
    <sap:tableCode>OITM</sap:tableCode>
    <sap:businessProcess>Item Master</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OITW">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#MasterData"/>
    <rdfs:label xml:lang="en">OITW (Item Warehouse Inventory)</rdfs:label>
    <rdfs:comment xml:lang="en">Item Warehouse: Item quantities per warehouse location. Updated by inventory transactions.</rdfs:comment>
    <sap:tableCode>OITW</sap:tableCode>
    <sap:businessProcess>Warehouse Stock</sap:businessProcess>
  </owl:Class>

  <!-- ========================================
       PRODUCTION CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OITT">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#ProductionDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#MasterData"/>
    <rdfs:label xml:lang="en">OITT (Bill of Material - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">BOM Header: Defines components needed to produce finished product. Links item (OITM) to its components via ITT1 lines.</rdfs:comment>
    <sap:tableCode>OITT</sap:tableCode>
    <sap:businessProcess>Bill of Material</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#ITT1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#ProductionDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">ITT1 (Bill of Material - Components)</rdfs:label>
    <rdfs:comment xml:lang="en">BOM Components: Individual component items required for production</rdfs:comment>
    <sap:tableCode>ITT1</sap:tableCode>
    <sap:businessProcess>BOM Component</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OWOR">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#ProductionDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OWOR (Production Order - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Production Order Header: Manages manufacturing execution. Produces finished items by consuming component items based on BOM.</rdfs:comment>
    <sap:tableCode>OWOR</sap:tableCode>
    <sap:businessProcess>Production Order</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#WOR1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#ProductionDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">WOR1 (Production Order - Components)</rdfs:label>
    <rdfs:comment xml:lang="en">Production Order Components: Component items to be consumed in production</rdfs:comment>
    <sap:tableCode>WOR1</sap:tableCode>
    <sap:businessProcess>Production Component</sap:businessProcess>
  </owl:Class>

  <!-- ========================================
       INVENTORY MOVEMENT CLASSES
       ======================================== -->
  
  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OIGN">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#InventoryDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OIGN (Goods Receipt - Header)</rdfs:label>
    <rdfs:comment xml:lang="en">Goods Receipt Header: Increases inventory without purchase order</rdfs:comment>
    <sap:tableCode>OIGN</sap:tableCode>
    <sap:businessProcess>Goods Receipt</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#IGN1">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#InventoryDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#DetailLine"/>
    <rdfs:label xml:lang="en">IGN1 (Goods Receipt - Lines)</rdfs:label>
    <rdfs:comment xml:lang="en">Goods Receipt Line Items</rdfs:comment>
    <sap:tableCode>IGN1</sap:tableCode>
    <sap:businessProcess>Goods Receipt Detail</sap:businessProcess>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/sap-b1/ontology#OIGE">
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#InventoryDocument"/>
    <rdfs:subClassOf rdf:resource="http://example.org/sap-b1/ontology#HeaderDocument"/>
    <rdfs:label xml:lang="en">OIGE (Goods Issue - Header)</rdfs:label>
