<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>SAP Customer Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling the SAP Customer module metadata, including field definitions, data types, permissions, operations, relationships, and business rules for customer record management.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field within the SAP Customer module, containing metadata about field properties, permissions, and behavior.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system, typically read-only and not customizable by users.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field that can be customized, modified, and configured according to business requirements.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must contain a value before a record can be saved.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that can be viewed but not modified by users.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Searchable Field</rdfs:label>
    <rdfs:comment>A field that can be used in search queries to locate records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FilterableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Filterable Field</rdfs:label>
    <rdfs:comment>A field that can be used to filter record lists based on specific criteria.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SortableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Sortable Field</rdfs:label>
    <rdfs:comment>A field that can be used to sort records in ascending or descending order.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Represents the type of data that can be stored in a field, such as text, number, date, or picklist.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Text Data Type</rdfs:label>
    <rdfs:comment>A data type for storing alphanumeric text strings.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Picklist Data Type</rdfs:label>
    <rdfs:comment>A data type that restricts values to a predefined list of options.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Email Data Type</rdfs:label>
    <rdfs:comment>A data type specifically for storing email addresses with validation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>A data type for storing date and time values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Numeric Data Type</rdfs:label>
    <rdfs:comment>A data type for storing numeric values including integers and decimals.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DoubleDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#NumericDataType"/>
    <rdfs:label>Double Data Type</rdfs:label>
    <rdfs:comment>A numeric data type for storing decimal numbers with specified precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#NumericDataType"/>
    <rdfs:label>BigInt Data Type</rdfs:label>
    <rdfs:comment>A numeric data type for storing large integer values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>A data type for storing true/false values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Lookup Data Type</rdfs:label>
    <rdfs:comment>A data type that references records from another module or entity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>A specialized lookup data type for referencing user or owner records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FormulaDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Formula Data Type</rdfs:label>
    <rdfs:comment>A data type that computes values based on expressions and other field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileImageDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Profile Image Data Type</rdfs:label>
    <rdfs:comment>A data type for storing image files associated with records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PickListValue">
    <rdfs:label>PickList Value</rdfs:label>
    <rdfs:comment>Represents an individual option within a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Represents the lifecycle status of a record (Available, Draft, Trash).</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PaymentTerms">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Payment Terms</rdfs:label>
    <rdfs:comment>Represents payment term options for customer transactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UnsubscribeMode">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Unsubscribe Mode</rdfs:label>
    <rdfs:comment>Represents the method by which a customer unsubscribed from communications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>Represents a user profile with specific permissions and access rights to fields and modules.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full administrative access and permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user access and permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A specialized profile for dealer users with specific access rights.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>Represents access rights granted to profiles for specific fields or operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Permission that allows both viewing and modifying data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Only Permission</rdfs:label>
    <rdfs:comment>Permission that allows only viewing data without modification rights.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Operation">
    <rdfs:label>Operation</rdfs:label>
    <rdfs:comment>Represents an action that can be performed on a field or record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Create Operation</rdfs:label>
    <rdfs:comment>An operation that creates new records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Update Operation</rdfs:label>
    <rdfs:comment>An operation that modifies existing records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>Represents different contexts in which a field can be displayed or edited.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DetailView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Detail View</rdfs:label>
    <rdfs:comment>A view showing complete record details.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Edit View</rdfs:label>
    <rdfs:comment>A view allowing record modification.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Create View</rdfs:label>
    <rdfs:comment>A view for creating new records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuickCreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Quick Create View</rdfs:label>
    <rdfs:comment>A simplified view for rapid record creation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Represents the visual arrangement and organization of fields in the user interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>Standard Layout</rdfs:label>
    <rdfs:comment>The default layout configuration for the module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>Represents a functional module within the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SAPCustomerModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>SAP Customer Module</rdfs:label>
    <rdfs:comment>The module for managing SAP customer records and related data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Record">
    <rdfs:label>Record</rdfs:label>
    <rdfs:comment>Represents an individual data record within a module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomerRecord">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Record"/>
    <rdfs:label>Customer Record</rdfs:label>
    <rdfs:comment>A record representing a customer entity with associated data fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Represents a system user who can perform operations on records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordOwner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Record Owner</rdfs:label>
    <rdfs:comment>A user who owns and has primary responsibility for a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordCreator">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Record Creator</rdfs:label>
    <rdfs:comment>A user who created a specific record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordModifier">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Record Modifier</rdfs:label>
    <rdfs:comment>A user who last modified a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>Represents a property or characteristic of a field that can be customized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FieldLabel">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Field Label</rdfs:label>
    <rdfs:comment>The display label for a field that can be customized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MaxLength">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Max Length</rdfs:label>
    <rdfs:comment>The maximum number of characters or digits allowed in a field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UniqueConstraint">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Unique Constraint</rdfs:label>
    <rdfs:comment>A constraint ensuring field values are unique across records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tooltip">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CustomizableProperty"/>
    <rdfs:label>Tooltip</rdfs:label>
    <rdfs:comment>Help text displayed when hovering over a field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Formula">
    <rdfs:label>Formula</rdfs:label>
    <rdfs:comment>Represents a computational expression used to derive field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextFormula">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Formula"/>
    <rdfs:label>Text Formula</rdfs:label>
    <rdfs:comment>A formula that returns text values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericFormula">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Formula"/>
    <rdfs:label>Numeric Formula</rdfs:label>
    <rdfs:comment>A formula that returns numeric values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>Represents configuration for parsing email content to populate fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Webhook">
    <rdfs:label>Webhook</rdfs:label>
    <rdfs:comment>Represents a webhook configuration for field-level event notifications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HistoryTracking">
    <rdfs:label>History Tracking</rdfs:label>
    <rdfs:comment>Represents configuration for tracking changes to field values over time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MassUpdate">
    <rdfs:label>Mass Update</rdfs:label>
    <rdfs:comment>Represents the capability to update multiple records simultaneously.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Blueprint">
    <rdfs:label>Blueprint</rdfs:label>
    <rdfs:comment>Represents a workflow or process automation configuration.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessCard">
    <rdfs:label>Business Card</rdfs:label>
    <rdfs:comment>Represents a compact display format for record information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Encryption">
    <rdfs:label>Encryption</rdfs:label>
    <rdfs:comment>Represents encryption configuration for sensitive field data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AutoNumber">
    <rdfs:label>Auto Number</rdfs:label>
    <rdfs:comment>Represents automatic number generation configuration for fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Separator">
    <rdfs:label>Separator</rdfs:label>
    <rdfs:comment>Represents a visual separator in the layout to organize fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ColourCode">
    <rdfs:label>Colour Code</rdfs:label>
    <rdfs:comment>Represents color coding configuration for visual field differentiation.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties (Relationships)     -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasDataType">
    <rdfs:label>has data type</rdfs:label>
    <rdfs:comment>Relates a field to its data type specification.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#DataType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isDataTypeOf">
    <rdfs:label>is data type of</rdfs:label>
    <rdfs:comment>Inverse of hasDataType.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasDataType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasPickListValue">
    <rdfs:label>has picklist value</rdfs:label>
    <rdfs:comment>Relates a picklist field to its available values.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#PickListValue"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isPickListValueOf">
    <rdfs:label>is picklist value of</rdfs:label>
    <rdfs:comment>Inverse of hasPickListValue.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasPickListValue"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasProfile">
    <rdfs:label>has profile</rdfs:label>
    <rdfs:comment>Relates a field to profiles that have access to it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Profile"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasAccessToField">
    <rdfs:label>has access to field</rdfs:label>
    <rdfs:comment>Inverse of hasProfile.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasProfile"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasPermission">
    <rdfs:label>has permission</rdfs:label>
    <rdfs:comment>Relates a profile to its permission level for a field.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Permission"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isPermissionOf">
    <rdfs:label>is permission of</rdfs:label>
    <rdfs:comment>Inverse of hasPermission.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Profile"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasPermission"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#supportsOperation">
    <rdfs:label>supports operation</rdfs:label>
    <rdfs:comment>Relates a field to operations that can be performed on it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Operation"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isOperationOf">
