<!-- ===================================== -->
<!-- Property Characteristics              -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#subCategoryOf">
    <rdfs:label>sub category of</rdfs:label>
    <rdfs:comment>Relates a product category to its parent category in a hierarchy.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdfs:range rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasSubCategory">
    <rdfs:label>has sub category</rdfs:label>
    <rdfs:comment>Inverse relationship indicating child categories.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdfs:range rdf:resource="http://example.org/ontology#ProductCategory"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#subCategoryOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#manages">
    <rdfs:label>manages</rdfs:label>
    <rdfs:comment>Relates a user to fields or records they manage.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#User"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#managedBy">
    <rdfs:label>managed by</rdfs:label>
    <rdfs:comment>Inverse relationship indicating who manages a field.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#manages"/>
</owl:ObjectProperty>

<!-- ===================================== -->
<!-- Additional Restrictions and Axioms    -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isSystemMandatory"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must have a value before a record can be saved.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OptionalField">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isSystemMandatory"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Optional Field</rdfs:label>
    <rdfs:comment>A field that can be left empty when saving a record.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#MandatoryField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditableField">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isReadOnly"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Editable Field</rdfs:label>
    <rdfs:comment>A field that can be modified by users with appropriate permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NonEditableField">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isReadOnly"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Non-Editable Field</rdfs:label>
    <rdfs:comment>A field that cannot be modified after creation.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#EditableField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ActiveProduct">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isProductActive"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Active Product</rdfs:label>
    <rdfs:comment>A product that is currently active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#InactiveProduct">
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#isProductActive"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
        </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:label>Inactive Product</rdfs:label>
    <rdfs:comment>A product that is not currently active.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#ActiveProduct"/>
</owl:Class>

<!-- Property Chain Axioms -->

<rdf:Description rdf:about="http://example.org/ontology#indirectlyManagedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>indirectly managed by</rdfs:label>
    <rdfs:comment>Relates a product to users who manage it through field ownership.</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <rdf:Description rdf:about="http://example.org/ontology#hasField"/>
        <rdf:Description rdf:about="http://example.org/ontology#managedBy"/>
    </owl:propertyChainAxiom>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/ontology#hasPermissionForField">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has permission for field</rdfs:label>
    <rdfs:comment>Relates a user profile directly to fields through permissions.</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <rdf:Description rdf:about="http://example.org/ontology#hasPermission"/>
        <rdf:Description rdf:about="http://example.org/ontology#appliesTo"/>
    </owl:propertyChainAxiom>
</rdf:Description>

</rdf:RDF>