<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Product Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling product management fields, their properties, relationships, permissions, and operational characteristics within a CRM system context. This ontology captures field metadata, user profiles, layout associations, data types, validation rules, and system behaviors.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field within the product management system. Fields capture specific attributes of products and have associated metadata including type, permissions, validation rules, and display properties.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is automatically managed by the system and typically not customizable by users. Examples include Created Time, Modified Time, and Record Status.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A field created by users or administrators to extend the standard data model with domain-specific attributes.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#SystemField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Standard Field</rdfs:label>
    <rdfs:comment>A predefined field that comes with the system by default and represents common product attributes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that references another entity or record, establishing relationships between different data objects.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:label>Owner Lookup Field</rdfs:label>
    <rdfs:comment>A specialized lookup field that references the owner or responsible user for a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Text Field</rdfs:label>
    <rdfs:comment>A field that stores textual data with specified length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Text Area Field</rdfs:label>
    <rdfs:comment>A field that stores large amounts of text, typically used for descriptions or notes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Boolean Field</rdfs:label>
    <rdfs:comment>A field that stores true/false values, typically represented as checkboxes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field that allows selection from a predefined list of values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MultiselectPicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PicklistField"/>
    <rdfs:label>Multiselect Picklist Field</rdfs:label>
    <rdfs:comment>A picklist field that allows selection of multiple values simultaneously.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CurrencyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Currency Field</rdfs:label>
    <rdfs:comment>A field that stores monetary values with currency-specific formatting and precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>DateTime Field</rdfs:label>
    <rdfs:comment>A field that stores date and time information with timezone awareness.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TagField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Tag Field</rdfs:label>
    <rdfs:comment>A field that stores multiple tags or labels for categorization and filtering purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Product">
    <rdfs:label>Product</rdfs:label>
    <rdfs:comment>Represents a product entity in the system, containing all associated field values and metadata.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UserProfile">
    <rdfs:label>User Profile</rdfs:label>
    <rdfs:comment>Represents a user profile or role that defines access permissions and capabilities within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full administrative privileges and unrestricted access to system features.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user permissions for regular operational tasks.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A specialized profile for dealer users with specific permissions tailored to dealer operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>Represents an access permission that defines what operations a user profile can perform on a field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Permission that allows both reading and modifying field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Only Permission</rdfs:label>
    <rdfs:comment>Permission that allows only reading field values without modification capabilities.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#ReadWritePermission"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Represents a user interface layout that organizes and displays fields in a specific arrangement.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LayoutAssociation">
    <rdfs:label>Layout Association</rdfs:label>
    <rdfs:comment>Represents the association between a field and a layout, defining where and how the field appears.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OperationType">
    <rdfs:label>Operation Type</rdfs:label>
    <rdfs:comment>Represents a type of operation that can be performed on a field, such as create, update, or view.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>Operation type for creating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>Operation type for updating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APICreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>Operation type for creating records through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APIUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>Operation type for updating records through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>Represents different view contexts where a field can be displayed or edited.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DetailView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Detail View</rdfs:label>
    <rdfs:comment>View type for displaying complete record details.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Edit View</rdfs:label>
    <rdfs:comment>View type for editing record values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Create View</rdfs:label>
    <rdfs:comment>View type for creating new records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuickCreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateView"/>
    <rdfs:label>Quick Create View</rdfs:label>
    <rdfs:comment>Simplified view type for rapid record creation with essential fields only.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistValue">
    <rdfs:label>Picklist Value</rdfs:label>
    <rdfs:comment>Represents an individual option within a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Represents the data type classification of a field, defining what kind of values it can store.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ValidationRule">
    <rdfs:label>Validation Rule</rdfs:label>
    <rdfs:comment>Represents a validation constraint that field values must satisfy.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UniquenessConstraint">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ValidationRule"/>
    <rdfs:label>Uniqueness Constraint</rdfs:label>
    <rdfs:comment>Validation rule requiring field values to be unique across all records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryConstraint">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ValidationRule"/>
    <rdfs:label>Mandatory Constraint</rdfs:label>
    <rdfs:comment>Validation rule requiring a field to have a value before record can be saved.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LengthConstraint">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ValidationRule"/>
    <rdfs:label>Length Constraint</rdfs:label>
    <rdfs:comment>Validation rule defining maximum or minimum length for text values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>Represents a property of a field that can be customized by administrators.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>Component that defines how field values can be extracted from email content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CurrencyConfiguration">
    <rdfs:label>Currency Configuration</rdfs:label>
    <rdfs:comment>Configuration settings for currency fields including precision and rounding options.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RoundingOption">
    <rdfs:label>Rounding Option</rdfs:label>
    <rdfs:comment>Defines how currency values should be rounded during calculations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TaxConfiguration">
    <rdfs:label>Tax Configuration</rdfs:label>
    <rdfs:comment>Configuration for tax-related fields and calculations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Represents the lifecycle status of a record within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Status indicating a record is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>Status indicating a record has been deleted and is in the trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Status indicating a record is in draft state and not yet finalized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProductCategory">
    <rdfs:label>Product Category</rdfs:label>
    <rdfs:comment>Represents a classification category for products.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HardwareCategory">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdfs:label>Hardware Category</rdfs:label>
    <rdfs:comment>Category for physical hardware products.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SoftwareCategory">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdfs:label>Software Category</rdfs:label>
    <rdfs:comment>Category for software products and applications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SuiteCategory">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ProductCategory"/>
    <rdfs:label>Suite Category</rdfs:label>
    <rdfs:comment>Category for product suites containing multiple integrated components.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CRMApplicationCategory">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SoftwareCategory"/>
    <rdfs:label>CRM Application Category</rdfs:label>
    <rdfs:comment>Category specifically for CRM application products.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Represents a user of the system who can own, create, or modify records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Webhook">
    <rdfs:label>Webhook</rdfs:label>
    <rdfs:comment>Represents a webhook configuration that triggers external notifications when field values change.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HistoryTracking">
    <rdfs:label>History Tracking</rdfs:label>
    <rdfs:comment>Configuration for tracking historical changes to field values over time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>Represents a functional module within the system that groups related fields and functionality.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties (Relationships)     -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasField">
    <rdfs:label>has field</rdfs:label>
    <rdfs:comment>Relates a product or module to its constituent fields.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#fieldOf">
    <rdfs:label>field of</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which product or module a field belongs to.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Product"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasField"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasPermission">
    <rdfs:label>has permission</rdfs:label>
    <rdfs:comment>Relates a user profile to the permissions it holds for specific fields.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Permission"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#permissionGrantedTo">
    <rdfs:label>permission granted to</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which profiles have been granted a permission.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:range rdf:resource="http://example.org/ontology#UserProfile"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasPermission"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#appliesTo">
    <rdfs:label>applies to</rdfs:label>
    <rdfs:comment>Relates a permission to the field it governs.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#governedBy">
    <rdfs:label>governed by</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which permissions govern a field.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Permission"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#appliesTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#associatedWithLayout">
    <rdfs:label>associated with layout</rdfs:label>
    <rdfs:comment>Relates a field to the layouts where it appears.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Layout"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#containsField">
    <rdfs:label>contains field</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which fields are contained in a layout.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#associatedWithLayout"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasLayoutAssociation">
    <rdfs:label>has layout association</rdfs:label>
    <rdfs:comment>Relates a field to its layout association configuration.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#LayoutAssociation"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#supportsOperation">
    <rdfs:label>supports operation</rdfs:label>
    <rdfs:comment>Relates a field to the operations that can be performed on it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#OperationType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#operationSupportedBy">
    <rdfs:label>operation supported by</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which fields support an operation.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#supportsOperation"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasViewType">
    <rdfs:label>has view type</rdfs:label>
    <rdfs:comment>Relates a field to the view types where it can be displayed.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#ViewType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#displaysField">
    <rdfs:label>displays field</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which fields are displayed in a view type.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasViewType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasPicklistValue">
    <rdfs:label>has picklist value</rdfs:label>
    <rdfs:comment>Relates a picklist field to its available values.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#PicklistField"/>
    <rdfs:range rdf:resource="http://example.org/ontology#PicklistValue"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#valueOfPicklist">
    <rdfs:label>value of picklist</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which picklist a value belongs to.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#PicklistValue"/>
    <rdfs:range rdf:resource="http://example.org/ontology#PicklistField"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasPicklistValue"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasDataType">
    <rdfs:label>has data type</rdfs:label>
    <rdfs:comment>Relates a field to its data type classification.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#DataType"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#dataTypeOf">
    <rdfs:label>data type of</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which fields use a data type.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasDataType"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasValidationRule">
    <rdfs:label>has validation rule</rdfs:label>
    <rdfs:comment>Relates a field to validation rules that constrain its values.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#ValidationRule"/>
