  </rdf:Description>
  <!-- =========================== Notes about dataset semantics (encoded as annotations) =========================== -->
  <owl:AnnotationProperty rdf:about="http://example.org/ontology#sourceDataset"/>
  <owl:Axiom>
    <owl:annotatedSource rdf:resource="http://example.org/ontology#Organization"/>
    <owl:annotatedProperty rdf:resource="http://example.org/ontology#sourceDataset"/>
    <owl:annotatedTarget rdf:datatype="http://www.w3.org/2001/XMLSchema#string">azure_devops_light_dump
 1.json</owl:annotatedTarget>
  </owl:Axiom>
