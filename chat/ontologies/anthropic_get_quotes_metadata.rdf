<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Quote Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling the domain of quote management, including fields, operations, permissions, relationships, and business processes derived from CRM metadata.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===== Core Entity Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Quote">
    <rdfs:label>Quote</rdfs:label>
    <rdfs:comment>A formal business document representing a proposed price and terms for products or services offered to a customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>A data element or attribute that stores specific information within a business record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system, typically read-only and used for tracking metadata.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field created to capture domain-specific information beyond standard fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Standard Field</rdfs:label>
    <rdfs:comment>A predefined field provided by the system as part of the default data model.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Text Field</rdfs:label>
    <rdfs:comment>A field that stores textual data with specified length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Numeric Field</rdfs:label>
    <rdfs:comment>A field that stores numerical values including integers, decimals, and currency amounts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>DateTime Field</rdfs:label>
    <rdfs:comment>A field that stores temporal information including dates and timestamps.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field that allows selection from a predefined list of values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that establishes a relationship to another entity by referencing its record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FormulaField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Formula Field</rdfs:label>
    <rdfs:comment>A computed field whose value is automatically calculated based on an expression.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CurrencyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#NumericField"/>
    <rdfs:label>Currency Field</rdfs:label>
    <rdfs:comment>A numeric field specifically designed to store monetary values with currency type and precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AutoNumberField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SystemField"/>
    <rdfs:label>Auto Number Field</rdfs:label>
    <rdfs:comment>A system-generated field that automatically assigns sequential or formatted unique identifiers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextField"/>
    <rdfs:label>Text Area Field</rdfs:label>
    <rdfs:comment>A multi-line text field capable of storing large amounts of textual content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Boolean Field</rdfs:label>
    <rdfs:comment>A field that stores true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SubformField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Subform Field</rdfs:label>
    <rdfs:comment>A field that contains a collection of related records embedded within the parent record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AddressField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Address Field</rdfs:label>
    <rdfs:comment>A composite field that stores structured address information including street, city, state, and country.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:label>Owner Lookup Field</rdfs:label>
    <rdfs:comment>A specialized lookup field that references the user or entity responsible for the record.</rdfs:comment>
</owl:Class>

<!-- ===== Business Entity Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Deal">
    <rdfs:label>Deal</rdfs:label>
    <rdfs:comment>A potential sales opportunity or transaction being pursued with a customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual person associated with a business relationship or transaction.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>A company or organization that is a customer or potential customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Vendor">
    <rdfs:label>Vendor</rdfs:label>
    <rdfs:comment>A supplier or dealer who provides products or services to the organization.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Invoice">
    <rdfs:label>Invoice</rdfs:label>
    <rdfs:comment>A billing document requesting payment for products or services delivered.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SalesOrder">
    <rdfs:label>Sales Order</rdfs:label>
    <rdfs:comment>A confirmed order from a customer for products or services to be delivered.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuotedItem">
    <rdfs:label>Quoted Item</rdfs:label>
    <rdfs:comment>A line item within a quote representing a specific product or service being offered.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Product">
    <rdfs:label>Product</rdfs:label>
    <rdfs:comment>A good or service offered for sale by the organization.</rdfs:comment>
</owl:Class>

<!-- ===== User and Permission Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A person who has access to the system and can perform operations based on assigned permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>A collection of permissions and settings that define what a user can access and modify.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full system access and administrative privileges.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user permissions for normal business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A profile designed for dealer or vendor users with specific access rights.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>An authorization that grants or restricts access to specific system functions or data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Permission</rdfs:label>
    <rdfs:comment>Permission to view data without the ability to modify it.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Write Permission</rdfs:label>
    <rdfs:comment>Permission to create and modify data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read-Write Permission</rdfs:label>
    <rdfs:comment>Combined permission to both view and modify data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read-Only Permission</rdfs:label>
    <rdfs:comment>Restricted permission allowing only viewing of data without modification rights.</rdfs:comment>
</owl:Class>

<!-- ===== Operation and Process Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Operation">
    <rdfs:label>Operation</rdfs:label>
    <rdfs:comment>An action or process that can be performed on data within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Create Operation</rdfs:label>
    <rdfs:comment>An operation that generates new records in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Update Operation</rdfs:label>
    <rdfs:comment>An operation that modifies existing records in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APICreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the application programming interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APIUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the application programming interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MassUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Mass Update Operation</rdfs:label>
    <rdfs:comment>An operation that updates multiple records simultaneously.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ConversionOperation">
    <rdfs:label>Conversion Operation</rdfs:label>
    <rdfs:comment>An operation that transforms one type of record into another related type.</rdfs:comment>
</owl:Class>

<!-- ===== Configuration and Metadata Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>A visual arrangement and organization of fields and sections in the user interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>A functional component of the system that manages a specific type of business data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>A specification of how and where a field can be displayed or interacted with.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistValue">
    <rdfs:label>Picklist Value</rdfs:label>
    <rdfs:comment>An individual option available for selection in a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Currency">
    <rdfs:label>Currency</rdfs:label>
    <rdfs:comment>A monetary unit used for financial transactions and calculations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Formula">
    <rdfs:label>Formula</rdfs:label>
    <rdfs:comment>A mathematical or logical expression used to compute field values automatically.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Webhook">
    <rdfs:label>Webhook</rdfs:label>
    <rdfs:comment>An automated notification mechanism that triggers external systems when data changes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>A system component that extracts and processes information from email messages.</rdfs:comment>
</owl:Class>

<!-- ===== Status and State Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#QuoteStage">
    <rdfs:label>Quote Stage</rdfs:label>
    <rdfs:comment>A phase in the lifecycle of a quote representing its current status in the sales process.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Draft Stage</rdfs:label>
    <rdfs:comment>Initial stage where the quote is being prepared and not yet finalized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NegotiationStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Negotiation Stage</rdfs:label>
    <rdfs:comment>Stage where terms and pricing are being discussed with the customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DeliveredStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Delivered Stage</rdfs:label>
    <rdfs:comment>Stage where the quote has been sent to the customer for review.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OnHoldStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>On Hold Stage</rdfs:label>
    <rdfs:comment>Stage where the quote process is temporarily paused.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ConfirmedStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Confirmed Stage</rdfs:label>
    <rdfs:comment>Stage where the customer has accepted the quote terms.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ClosedWonStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Closed Won Stage</rdfs:label>
    <rdfs:comment>Final successful stage where the quote has resulted in a sale.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ClosedLostStage">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#QuoteStage"/>
    <rdfs:label>Closed Lost Stage</rdfs:label>
    <rdfs:comment>Final unsuccessful stage where the quote did not result in a sale.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>The current state of a record in the system lifecycle.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Status indicating the record is active and accessible.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>Status indicating the record has been deleted and is in the recycle bin.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Status indicating the record is being prepared and not yet published.</rdfs:comment>
</owl:Class>

<!-- ===== Shipping and Logistics Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Carrier">
    <rdfs:label>Carrier</rdfs:label>
    <rdfs:comment>A shipping company or service provider responsible for delivering goods.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Address">
    <rdfs:label>Address</rdfs:label>
    <rdfs:comment>A structured representation of a physical location including street, city, state, and country.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BillingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Billing Address</rdfs:label>
    <rdfs:comment>The address where invoices and payment requests are sent.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ShippingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Shipping Address</rdfs:label>
    <rdfs:comment>The address where goods are to be delivered.</rdfs:comment>
</owl:Class>

<!-- ===== Financial Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#FinancialAmount">
    <rdfs:label>Financial Amount</rdfs:label>
    <rdfs:comment>A monetary value associated with a transaction or calculation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SubTotal">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialAmount"/>
    <rdfs:label>Sub Total</rdfs:label>
    <rdfs:comment>The sum of all line items before taxes, discounts, and adjustments.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Discount">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialAmount"/>
    <rdfs:label>Discount</rdfs:label>
    <rdfs:comment>A reduction in price offered to the customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tax">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialAmount"/>
    <rdfs:label>Tax</rdfs:label>
    <rdfs:comment>Government-imposed charges added to the transaction total.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Adjustment">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialAmount"/>
    <rdfs:label>Adjustment</rdfs:label>
    <rdfs:comment>A manual correction or modification to the calculated total.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#GrandTotal">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialAmount"/>
    <rdfs:label>Grand Total</rdfs:label>
    <rdfs:comment>The final amount due after all calculations including taxes, discounts, and adjustments.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ExchangeRate">
    <rdfs:label>Exchange Rate</rdfs:label>
    <rdfs:comment>The conversion rate between different currencies.</rdfs:comment>
</owl:Class>

<!-- ===== Team and Organization Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Team">
    <rdfs:label>Team</rdfs:label>
    <rdfs:comment>A group of users who collaborate on business processes and share access to records.</rdfs:comment>
</owl:Class>

<!-- ===== Object Properties - Core Relationships ===== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasField">
    <rdfs:label>has field</rdfs:label>
    <rdfs:comment>Relates an entity to a field that stores its data.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Quote"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isFieldOf">
    <rdfs:label>is field of</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which entity a field belongs to.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Quote"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasField"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#referencesModule">
    <rdfs:label>references module</rdfs:label>
    <rdfs:comment>Indicates that a lookup field references records from a specific module.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Module"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isReferencedBy">
    <rdfs:label>is referenced by</rdfs:label>
    <rdfs:comment>Inverse relationship showing which fields reference a module.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:range rdf:resource="http://example.org/ontology#LookupField"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#referencesModule"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#associatedWithDeal">
    <rdfs:label>associated with deal</rdfs:label>
    <rdfs:comment>Links a quote to the sales opportunity it is related to.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Quote"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Deal"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasQuote">
    <rdfs:label>has quote</rdfs:label>
    <rdfs:comment>Inverse relationship showing which quotes are associated with a deal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Deal"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Quote"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#associatedWithDeal"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#associatedWithContact">
