<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Task Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling task management fields, their properties, relationships, permissions, and operational characteristics within a CRM system context. This ontology captures entities such as tasks, fields, profiles, users, modules, layouts, and their complex interrelationships including permissions, operations, data types, and validation rules.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field within a module that captures specific information. Fields have various properties including data types, visibility settings, permissions, and operational characteristics.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is provided by the system by default and has predefined behavior and characteristics.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A field that has been created by users to extend the default system functionality with custom data capture requirements.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#SystemField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that establishes a relationship to records in another module, enabling cross-module data references.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:label>Owner Lookup Field</rdfs:label>
    <rdfs:comment>A specialized lookup field that references user or owner information, typically used for record ownership tracking.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field that provides a predefined list of values from which users can select.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
