<owl:DatatypeProperty rdf:about="http://example.org/ontology#isLocked">
    <rdfs:label>is locked</rdfs:label>
    <rdfs:comment>Indicates whether a record is locked from editing.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Task"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<!-- ===================================== -->
<!-- Additional Axioms and Restrictions    -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must have a value before a record can be saved.</rdfs:comment>
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#hasCustomizableProperty"/>
            <owl:someValuesFrom rdf:resource="http://example.org/ontology#MandatoryProperty"/>
        </owl:Restriction>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditableField">
    <rdfs:label>Editable Field</rdfs:label>
    <rdfs:comment>A field that can be modified by users with appropriate permissions.</rdfs:comment>
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://example.org/ontology#Field"/>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="http://example.org/ontology#isReadOnly"/>
                    <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CompletedTask">
    <rdfs:label>Completed Task</rdfs:label>
    <rdfs:comment>A task that has been finished and marked as complete.</rdfs:comment>
    <owl:equivalentClass>
        <owl:Restriction>
            <owl:onProperty rdf:resource="http://example.org/ontology#hasStatus"/>
            <owl:hasValue rdf:resource="http://example.org/ontology#CompletedStatus"/>
        </owl:Restriction>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HighPriorityTask">
    <rdfs:label>High Priority Task</rdfs:label>
    <rdfs:comment>A task with high or highest priority level.</rdfs:comment>
    <owl:equivalentClass>
        <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
                <owl:Restriction>
                    <owl:onProperty rdf:resource="http://example.org/ontology#hasPriority"/>
                    <owl:hasValue rdf:resource="http://example.org/ontology#HighPriority"/>
                </owl:Restriction>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="http://example.org/ontology#hasPriority"/>
                    <owl:hasValue rdf:resource="http://example.org/ontology#HighestPriority"/>
                </owl:Restriction>
            </owl:unionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<!-- Property Chains -->
<rdf:Description rdf:about="http://example.org/ontology#hasPermissionForField">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has permission for field</rdfs:label>
    <rdfs:comment>Relates a profile to fields it has permissions for through permission objects.</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <rdf:Description rdf:about="http://example.org/ontology#hasPermission"/>
        <rdf:Description rdf:about="http://example.org/ontology#appliesTo"/>
    </owl:propertyChainAxiom>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/ontology#userCanAccessField">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>user can access field</rdfs:label>
    <rdfs:comment>Relates a user to fields they can access through their profile permissions.</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <rdf:Description rdf:about="http://example.org/ontology#hasProfile"/>
        <rdf:Description rdf:about="http://example.org/ontology#hasPermission"/>
        <rdf:Description rdf:about="http://example.org/ontology#appliesTo"/>
    </owl:propertyChainAxiom>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/ontology#moduleContainsLookupToModule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>module contains lookup to module</rdfs:label>
    <rdfs:comment>Relates a module to other modules it references through lookup fields.</rdfs:comment>
    <owl:propertyChainAxiom rdf:parseType="Collection">
        <rdf:Description rdf:about="http://example.org/ontology#containsField"/>
        <rdf:Description rdf:about="http://example.org/ontology#lookupsTo"/>
    </owl:propertyChainAxiom>
</rdf:Description>

<!-- Symmetric Properties -->
<owl:ObjectProperty rdf:about="http://example.org/ontology#relatedModule">
    <rdfs:label>related module</rdfs:label>
    <rdfs:comment>Relates modules that are connected through lookup relationships.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Module"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
</owl:ObjectProperty>

<!-- Transitive Properties -->
<owl:ObjectProperty rdf:about="http://example.org/ontology#hasSubProfile">
    <rdfs:label>has sub profile</rdfs:label>
    <rdfs:comment>Relates a profile to profiles that inherit from it in a hierarchy.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Profile"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
</owl:ObjectProperty>

</rdf:RDF>