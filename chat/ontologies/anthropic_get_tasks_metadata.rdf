    <rdfs:comment>A specialized field that captures recurrence rules for repeating activities using RRULE format.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReminderField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Reminder Field</rdfs:label>
    <rdfs:comment>A field that captures reminder or alarm information for scheduled activities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TagField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
