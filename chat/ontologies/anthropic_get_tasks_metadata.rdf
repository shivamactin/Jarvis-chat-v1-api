    <rdfs:comment>A multi-line text field that can capture larger amounts of textual content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Boolean Field</rdfs:label>
    <rdfs:comment>A field that captures true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecurringActivityField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
