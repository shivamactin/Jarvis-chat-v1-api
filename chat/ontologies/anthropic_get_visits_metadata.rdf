<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Web Visitor Tracking and Analytics Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling web visitor tracking, page visits, user behavior analytics, advertising campaign tracking, and CRM integration for leads and contacts. This ontology captures the complete semantic structure of visitor tracking fields including temporal, behavioral, technical, and marketing attribution dimensions.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Visit">
    <rdfs:label>Visit</rdfs:label>
    <rdfs:comment>Represents a single visit session by a user to a website or web application. A visit encompasses all interactions, page views, and activities during a continuous browsing session.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebPage">
    <rdfs:label>Web Page</rdfs:label>
    <rdfs:comment>Represents a specific web page or URL that can be visited by users. Contains information about the page location, content, and metadata.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Visitor">
    <rdfs:label>Visitor</rdfs:label>
    <rdfs:comment>Represents an individual who visits a website. A visitor can be identified or anonymous and may be associated with CRM entities like leads or contacts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CRMEntity">
    <rdfs:label>CRM Entity</rdfs:label>
    <rdfs:comment>Abstract class representing entities in a Customer Relationship Management system that can be tracked and associated with web visits.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Lead">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CRMEntity"/>
    <rdfs:label>Lead</rdfs:label>
    <rdfs:comment>Represents a potential customer or prospect in the CRM system who has shown interest but has not yet been qualified as a contact.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CRMEntity"/>
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>Represents a qualified individual in the CRM system with whom the organization has an established relationship.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Represents a system user who can create, modify, or manage records in the system. Includes administrators, standard users, and dealers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UserProfile">
    <rdfs:label>User Profile</rdfs:label>
    <rdfs:comment>Represents a user profile with specific permissions and access rights within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>User profile with full administrative privileges and read-write access to all system features.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>Standard user profile with typical read-write permissions for regular business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UserProfile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>Specialized user profile for dealer users with specific permissions tailored to dealer operations.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Technical Infrastructure Classes      -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#TechnicalEnvironment">
    <rdfs:label>Technical Environment</rdfs:label>
    <rdfs:comment>Abstract class representing the technical context in which a visit occurs, including browser, operating system, and device information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Browser">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TechnicalEnvironment"/>
    <rdfs:label>Browser</rdfs:label>
    <rdfs:comment>Represents a web browser application used to access web pages.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#GoogleChrome">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Browser"/>
    <rdfs:label>Google Chrome</rdfs:label>
    <rdfs:comment>Google Chrome web browser.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MozillaFirefox">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Browser"/>
    <rdfs:label>Mozilla Firefox</rdfs:label>
    <rdfs:comment>Mozilla Firefox web browser.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MicrosoftInternetExplorer">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Browser"/>
    <rdfs:label>Microsoft Internet Explorer</rdfs:label>
    <rdfs:comment>Microsoft Internet Explorer web browser.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AppleSafari">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Browser"/>
    <rdfs:label>Apple Safari</rdfs:label>
    <rdfs:comment>Apple Safari web browser.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Opera">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Browser"/>
    <rdfs:label>Opera</rdfs:label>
    <rdfs:comment>Opera web browser.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OperatingSystem">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TechnicalEnvironment"/>
    <rdfs:label>Operating System</rdfs:label>
    <rdfs:comment>Represents an operating system platform on which a browser runs.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DesktopOperatingSystem">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperatingSystem"/>
    <rdfs:label>Desktop Operating System</rdfs:label>
    <rdfs:comment>Operating systems designed for desktop and laptop computers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MobileOperatingSystem">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperatingSystem"/>
    <rdfs:label>Mobile Operating System</rdfs:label>
    <rdfs:comment>Operating systems designed for mobile devices like smartphones and tablets.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MicrosoftWindows">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DesktopOperatingSystem"/>
    <rdfs:label>Microsoft Windows</rdfs:label>
    <rdfs:comment>Microsoft Windows operating system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AppleMacintosh">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DesktopOperatingSystem"/>
    <rdfs:label>Apple Macintosh</rdfs:label>
    <rdfs:comment>Apple Macintosh operating system (macOS).</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Linux">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DesktopOperatingSystem"/>
    <rdfs:label>Linux</rdfs:label>
    <rdfs:comment>Linux operating system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Android">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#MobileOperatingSystem"/>
    <rdfs:label>Android</rdfs:label>
    <rdfs:comment>Google Android mobile operating system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AppleIPhone">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#MobileOperatingSystem"/>
    <rdfs:label>Apple iPhone</rdfs:label>
    <rdfs:comment>Apple iPhone iOS operating system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Device">
    <rdfs:label>Device</rdfs:label>
    <rdfs:comment>Represents a physical device used to access web content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Computer">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Device"/>
    <rdfs:label>Computer</rdfs:label>
    <rdfs:comment>Desktop or laptop computer device.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MobileDevice">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Device"/>
    <rdfs:label>Mobile Device</rdfs:label>
    <rdfs:comment>Mobile device with full browser capabilities such as smartphones.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tablet">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Device"/>
    <rdfs:label>Tablet</rdfs:label>
    <rdfs:comment>Tablet device with full browser capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#IPAddress">
    <rdfs:label>IP Address</rdfs:label>
    <rdfs:comment>Represents an Internet Protocol address that identifies a device on a network.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UserAgent">
    <rdfs:label>User Agent</rdfs:label>
    <rdfs:comment>Represents the user agent string that identifies the browser, operating system, and device characteristics.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Traffic Source and Referral Classes   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#TrafficSource">
    <rdfs:label>Traffic Source</rdfs:label>
    <rdfs:comment>Abstract class representing the origin or source of web traffic to a site.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DirectTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>Direct Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives directly by typing the URL or using bookmarks, with no referrer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReferralTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>Referral Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives from another website through a link.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchEngineTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>Search Engine Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives from organic search engine results.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SocialMediaTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>Social Media Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives from social media platforms.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CampaignTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>Campaign Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives from marketing campaigns with tracking parameters.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdWordsTraffic">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TrafficSource"/>
    <rdfs:label>AdWords Traffic</rdfs:label>
    <rdfs:comment>Traffic that arrives from Google AdWords paid advertising campaigns.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Referrer">
    <rdfs:label>Referrer</rdfs:label>
    <rdfs:comment>Represents the referring website or URL that directed traffic to the current page.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchEngine">
    <rdfs:label>Search Engine</rdfs:label>
    <rdfs:comment>Represents a search engine platform that can drive traffic through organic or paid search results.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Google">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchEngine"/>
    <rdfs:label>Google</rdfs:label>
    <rdfs:comment>Google search engine.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Yahoo">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchEngine"/>
    <rdfs:label>Yahoo</rdfs:label>
    <rdfs:comment>Yahoo search engine.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Bing">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchEngine"/>
    <rdfs:label>Bing</rdfs:label>
    <rdfs:comment>Microsoft Bing search engine.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Baidu">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchEngine"/>
    <rdfs:label>Baidu</rdfs:label>
    <rdfs:comment>Baidu search engine.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchKeyword">
    <rdfs:label>Search Keyword</rdfs:label>
    <rdfs:comment>Represents a keyword or search term used in a search engine query that led to a visit.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Advertising and Campaign Classes      -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#AdvertisingCampaign">
    <rdfs:label>Advertising Campaign</rdfs:label>
    <rdfs:comment>Represents a structured marketing campaign designed to drive traffic and conversions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdCampaign">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#AdvertisingCampaign"/>
    <rdfs:label>Ad Campaign</rdfs:label>
    <rdfs:comment>A specific advertising campaign with defined goals, budget, and targeting.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdGroup">
    <rdfs:label>Ad Group</rdfs:label>
    <rdfs:comment>A collection of related ads within a campaign that share targeting criteria and keywords.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Advertisement">
    <rdfs:label>Advertisement</rdfs:label>
    <rdfs:comment>An individual advertisement or ad creative within an ad group.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdNetwork">
    <rdfs:label>Ad Network</rdfs:label>
    <rdfs:comment>Represents the advertising network through which ads are displayed.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchNetwork">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#AdNetwork"/>
    <rdfs:label>Search Network</rdfs:label>
    <rdfs:comment>Advertising network that displays ads on search engine results pages.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DisplayNetwork">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#AdNetwork"/>
    <rdfs:label>Display Network</rdfs:label>
    <rdfs:comment>Advertising network that displays banner and visual ads on partner websites.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchPartnerNetwork">
    <rdfs:label>Search Partner Network</rdfs:label>
    <rdfs:comment>Network of search partners that display ads alongside search results.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#GoogleSearch">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchPartnerNetwork"/>
    <rdfs:label>Google Search</rdfs:label>
    <rdfs:comment>Google's own search results pages.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchPartners">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SearchPartnerNetwork"/>
    <rdfs:label>Search Partners</rdfs:label>
    <rdfs:comment>Third-party search partners that display Google ads.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdClick">
    <rdfs:label>Ad Click</rdfs:label>
    <rdfs:comment>Represents a click event on an advertisement that leads to a website visit.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ClickType">
    <rdfs:label>Click Type</rdfs:label>
    <rdfs:comment>Represents the type or nature of an ad click, indicating which element was clicked.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HeadlineClick">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ClickType"/>
    <rdfs:label>Headline Click</rdfs:label>
    <rdfs:comment>Click on the main headline of an advertisement.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SitelinkClick">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ClickType"/>
    <rdfs:label>Sitelink Click</rdfs:label>
    <rdfs:comment>Click on a sitelink extension within an advertisement.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PhoneCallClick">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ClickType"/>
    <rdfs:label>Phone Call Click</rdfs:label>
    <rdfs:comment>Click that initiates a phone call from an advertisement.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Gclid">
    <rdfs:label>Gclid</rdfs:label>
    <rdfs:comment>Google Click Identifier - a unique tracking parameter added to URLs by Google Ads to track ad clicks and conversions.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Behavioral and Analytics Classes      -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#VisitorBehavior">
    <rdfs:label>Visitor Behavior</rdfs:label>
    <rdfs:comment>Abstract class representing behavioral patterns and metrics of website visitors.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PageView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#VisitorBehavior"/>
    <rdfs:label>Page View</rdfs:label>
    <rdfs:comment>Represents a single instance of a page being viewed during a visit.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VisitorType">
    <rdfs:label>Visitor Type</rdfs:label>
    <rdfs:comment>Classification of visitors based on their characteristics or relationship to the organization.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TimeMetric">
    <rdfs:label>Time Metric</rdfs:label>
    <rdfs:comment>Abstract class representing time-based measurements and metrics.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TimeSpent">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TimeMetric"/>
    <rdfs:label>Time Spent</rdfs:label>
    <rdfs:comment>Represents the duration of time a visitor spent on a page or during a visit.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VisitTime">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TimeMetric"/>
    <rdfs:label>Visit Time</rdfs:label>
    <rdfs:comment>Represents the specific date and time when a visit occurred.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Revenue">
    <rdfs:label>Revenue</rdfs:label>
    <rdfs:comment>Represents monetary value or revenue associated with a visit or conversion event.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Portal">
    <rdfs:label>Portal</rdfs:label>
    <rdfs:comment>Represents a web portal or customer portal through which visits can occur.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- System and Record Management Classes  -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#SystemRecord">
    <rdfs:label>System Record</rdfs:label>
    <rdfs:comment>Abstract class representing a record or data entity within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VisitRecord">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SystemRecord"/>
    <rdfs:label>Visit Record</rdfs:label>
    <rdfs:comment>A system record that captures and stores information about a website visit.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Represents the lifecycle status of a record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Record is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>Record has been deleted and is in the trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Record is in draft state and not yet finalized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Represents a user interface layout configuration for displaying records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>Standard Layout</rdfs:label>
    <rdfs:comment>The standard default layout for displaying visit records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field or attribute within a record structure.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Text Field</rdfs:label>
    <rdfs:comment>A field that stores text or string data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>DateTime Field</rdfs:label>
    <rdfs:comment>A field that stores date and time information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NumericField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Numeric Field</rdfs:label>
    <rdfs:comment>A field that stores numeric values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field that allows selection from a predefined list of values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that references another record or entity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MultiModuleLookup">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:label>Multi-Module Lookup</rdfs:label>
    <rdfs:comment>A lookup field that can reference records from multiple different modules.</rdfs:comment>
