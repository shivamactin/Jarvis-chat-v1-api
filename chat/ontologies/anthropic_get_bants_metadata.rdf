<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>BANT Analysis CRM Field Metadata Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling CRM field metadata for BANT (Budget, Authority, Need, Timeline) analysis system, including field definitions, profiles, permissions, layouts, data types, and business relationships.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===== Core Classes ===== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field in the CRM system with properties, constraints, and metadata.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system, typically read-only and non-customizable.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field that can be customized and configured according to business needs.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#SystemField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must be populated with a value before record submission.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that cannot be edited by users, typically system-generated or calculated.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Searchable Field</rdfs:label>
    <rdfs:comment>A field that can be used in search queries to find records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FilterableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Filterable Field</rdfs:label>
    <rdfs:comment>A field that can be used to filter and segment records in views and reports.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SortableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Sortable Field</rdfs:label>
    <rdfs:comment>A field that can be used to sort records in ascending or descending order.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Represents the type of data that a field can store and validate.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Text Data Type</rdfs:label>
    <rdfs:comment>A data type for storing textual information with specified length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Picklist Data Type</rdfs:label>
    <rdfs:comment>A data type that allows selection from a predefined list of values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MultiselectPicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PicklistDataType"/>
    <rdfs:label>Multiselect Picklist Data Type</rdfs:label>
    <rdfs:comment>A picklist that allows selection of multiple values simultaneously.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Lookup Data Type</rdfs:label>
    <rdfs:comment>A data type that creates a relationship to records in another module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>A specialized lookup that references the user who owns the record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>A data type for storing date and time information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Email Data Type</rdfs:label>
    <rdfs:comment>A data type for storing and validating email addresses.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PhoneDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Phone Data Type</rdfs:label>
    <rdfs:comment>A data type for storing and formatting phone numbers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextDataType"/>
    <rdfs:label>Text Area Data Type</rdfs:label>
    <rdfs:comment>A data type for storing multi-line text with larger character limits.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>A data type for storing true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>BigInt Data Type</rdfs:label>
    <rdfs:comment>A data type for storing large integer values, typically used for unique identifiers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LayoutDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Layout Data Type</rdfs:label>
    <rdfs:comment>A data type for associating records with specific page layouts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MultiselectLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Multiselect Lookup Data Type</rdfs:label>
    <rdfs:comment>A lookup that allows relationships to multiple records in another module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PickListValue">
    <rdfs:label>Pick List Value</rdfs:label>
    <rdfs:comment>Represents an individual option within a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UsedPickListValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Used Pick List Value</rdfs:label>
    <rdfs:comment>A picklist value that is currently active and available for selection.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UnusedPickListValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PickListValue"/>
    <rdfs:label>Unused Pick List Value</rdfs:label>
    <rdfs:comment>A picklist value that has been deactivated but retained for historical data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>Represents a user profile that defines permissions and access levels for fields and modules.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full system access and administrative privileges.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user access and limited administrative capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A specialized profile for dealer users with specific business access requirements.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>Represents the level of access granted to a profile for a specific field or resource.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Permission that allows both viewing and editing of field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Only Permission</rdfs:label>
    <rdfs:comment>Permission that allows only viewing of field values without editing capability.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Represents a page layout that defines the arrangement and visibility of fields for records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>Standard Layout</rdfs:label>
    <rdfs:comment>The default layout configuration for a module.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BANTAnalysisLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>BANT Analysis Layout</rdfs:label>
    <rdfs:comment>A specialized layout for conducting BANT (Budget, Authority, Need, Timeline) analysis.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>Represents the different contexts in which a field can be displayed or edited.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Create View</rdfs:label>
    <rdfs:comment>The view displayed when creating a new record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Edit View</rdfs:label>
    <rdfs:comment>The view displayed when editing an existing record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DetailView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Detail View</rdfs:label>
    <rdfs:comment>The view displayed when viewing record details in read mode.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuickCreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Quick Create View</rdfs:label>
    <rdfs:comment>A simplified view for rapid record creation with essential fields only.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>Represents a functional module in the CRM system that contains related records and fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BANTModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>BANT Module</rdfs:label>
    <rdfs:comment>A custom module for managing BANT analysis records and qualification processes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LeadsModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>Leads Module</rdfs:label>
    <rdfs:comment>The standard CRM module for managing lead records and prospect information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ContactsModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>Contacts Module</rdfs:label>
    <rdfs:comment>The standard CRM module for managing contact records and relationship information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OperationType">
    <rdfs:label>Operation Type</rdfs:label>
    <rdfs:comment>Represents the types of operations that can be performed on a field through different interfaces.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>Operation for creating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>Operation for updating records through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APICreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>Operation for creating records through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APIUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OperationType"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>Operation for updating records through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>Represents a property of a field that can be customized by administrators.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BANTRecord">
    <rdfs:label>BANT Record</rdfs:label>
    <rdfs:comment>Represents a BANT analysis record that qualifies leads based on Budget, Authority, Need, and Timeline.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LeadRecord">
    <rdfs:label>Lead Record</rdfs:label>
    <rdfs:comment>Represents a potential customer or prospect in the sales pipeline.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ContactRecord">
    <rdfs:label>Contact Record</rdfs:label>
    <rdfs:comment>Represents an individual person associated with an organization or account.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Represents the lifecycle state of a record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Status indicating the record is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Status indicating the record is in draft state and not yet finalized.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
