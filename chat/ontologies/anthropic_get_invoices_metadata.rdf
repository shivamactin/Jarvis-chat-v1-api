<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Invoice Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling invoice management systems, including field metadata, user profiles, permissions, data types, operations, and business processes related to invoice creation, modification, and lifecycle management.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Business Entity Classes          -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Invoice">
    <rdfs:label>Invoice</rdfs:label>
    <rdfs:comment>A commercial document issued by a seller to a buyer, relating to a sale transaction and indicating the products, quantities, and agreed prices for products or services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SalesOrder">
    <rdfs:label>Sales Order</rdfs:label>
    <rdfs:comment>A document generated by a seller upon receiving a purchase order from a buyer, confirming the sale of goods or services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>A business entity or organization that represents a customer, partner, or company in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual person associated with an account or business transaction.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#InvoicedItem">
    <rdfs:label>Invoiced Item</rdfs:label>
    <rdfs:comment>A line item within an invoice representing a specific product or service being billed.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Field and Metadata Classes            -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>A data field or attribute within a business entity that stores specific information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system, typically read-only or with restricted modification capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field that can be added to extend the standard data model.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Lookup Field</rdfs:label>
    <rdfs:comment>A field that references another entity or record, establishing a relationship between entities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupField"/>
    <rdfs:label>Owner Lookup Field</rdfs:label>
    <rdfs:comment>A specialized lookup field that identifies the owner or responsible party for a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AutoNumberField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#SystemField"/>
    <rdfs:label>Auto Number Field</rdfs:label>
    <rdfs:comment>A field that automatically generates sequential or formatted numbers for record identification.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Text Field</rdfs:label>
    <rdfs:comment>A field that stores textual data with specified length constraints.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Date Field</rdfs:label>
    <rdfs:comment>A field that stores date values without time components.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>DateTime Field</rdfs:label>
    <rdfs:comment>A field that stores both date and time information with precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CurrencyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Currency Field</rdfs:label>
    <rdfs:comment>A field that stores monetary values with currency type and precision specifications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Picklist Field</rdfs:label>
    <rdfs:comment>A field that allows selection from a predefined list of values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextField"/>
    <rdfs:label>Text Area Field</rdfs:label>
    <rdfs:comment>A multi-line text field for storing larger amounts of textual content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FormulaField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Formula Field</rdfs:label>
    <rdfs:comment>A computed field that derives its value from a formula or calculation based on other fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SubformField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Subform Field</rdfs:label>
    <rdfs:comment>A field that contains a collection of related records, typically displayed as a table within a form.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Boolean Field</rdfs:label>
    <rdfs:comment>A field that stores true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DoubleField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Double Field</rdfs:label>
    <rdfs:comment>A field that stores floating-point numeric values with decimal precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Big Integer Field</rdfs:label>
    <rdfs:comment>A field that stores large integer values, typically used for unique identifiers.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- User and Permission Classes           -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A person who interacts with the system and has assigned roles and permissions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>A collection of permissions and settings that define what a user can access and modify in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdministratorProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Administrator Profile</rdfs:label>
    <rdfs:comment>A profile with full system access and administrative capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Standard Profile</rdfs:label>
    <rdfs:comment>A profile with standard user permissions for regular business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DealerProfile">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Profile"/>
    <rdfs:label>Dealer Profile</rdfs:label>
    <rdfs:comment>A specialized profile for dealer or partner users with specific access rights.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>A specific access right or capability granted to a user or profile.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Permission</rdfs:label>
    <rdfs:comment>Permission to view or read data without modification capabilities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Write Permission</rdfs:label>
    <rdfs:comment>Permission to create or modify data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read-Write Permission</rdfs:label>
    <rdfs:comment>Combined permission to both view and modify data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ReadPermission"/>
    <rdfs:label>Read-Only Permission</rdfs:label>
    <rdfs:comment>Explicit permission that restricts access to viewing only, with no modification rights.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Operation and Process Classes         -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Operation">
    <rdfs:label>Operation</rdfs:label>
    <rdfs:comment>An action or process that can be performed on data or records in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Create Operation</rdfs:label>
    <rdfs:comment>An operation that creates new records or data entries.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Update Operation</rdfs:label>
    <rdfs:comment>An operation that modifies existing records or data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APICreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>A create operation performed through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APIUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>An update operation performed through the API interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MassUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Mass Update Operation</rdfs:label>
    <rdfs:comment>An operation that updates multiple records simultaneously.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Address and Location Classes          -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Address">
    <rdfs:label>Address</rdfs:label>
    <rdfs:comment>A physical or mailing address with street, city, state, postal code, and country components.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BillingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Billing Address</rdfs:label>
    <rdfs:comment>The address to which invoices and billing statements are sent.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ShippingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Shipping Address</rdfs:label>
    <rdfs:comment>The address to which goods or services are delivered.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Financial and Currency Classes        -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Currency">
    <rdfs:label>Currency</rdfs:label>
    <rdfs:comment>A system of money in general use in a particular country or economic context.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MonetaryAmount">
    <rdfs:label>Monetary Amount</rdfs:label>
    <rdfs:comment>A financial value expressed in a specific currency with precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ExchangeRate">
    <rdfs:label>Exchange Rate</rdfs:label>
    <rdfs:comment>The rate at which one currency can be exchanged for another.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tax">
    <rdfs:label>Tax</rdfs:label>
    <rdfs:comment>A compulsory financial charge or levy imposed on goods or services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Discount">
    <rdfs:label>Discount</rdfs:label>
    <rdfs:comment>A reduction in the price of goods or services.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Adjustment">
    <rdfs:label>Adjustment</rdfs:label>
    <rdfs:comment>A modification to the total amount, either positive or negative, to account for special circumstances.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Layout and UI Classes                 -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>A visual arrangement of fields and sections in a user interface form.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardLayout">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Layout"/>
    <rdfs:label>Standard Layout</rdfs:label>
    <rdfs:comment>The default layout configuration for displaying and editing records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>A specification of how and where a field can be displayed or edited in the user interface.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Status and State Classes              -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#InvoiceStatus">
    <rdfs:label>Invoice Status</rdfs:label>
    <rdfs:comment>The current state or stage of an invoice in its lifecycle.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreatedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#InvoiceStatus"/>
    <rdfs:label>Created Status</rdfs:label>
    <rdfs:comment>Status indicating the invoice has been created but not yet processed.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApprovedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#InvoiceStatus"/>
    <rdfs:label>Approved Status</rdfs:label>
    <rdfs:comment>Status indicating the invoice has been reviewed and approved.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DeliveredStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#InvoiceStatus"/>
    <rdfs:label>Delivered Status</rdfs:label>
    <rdfs:comment>Status indicating the invoice has been delivered to the customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CancelledStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#InvoiceStatus"/>
    <rdfs:label>Cancelled Status</rdfs:label>
    <rdfs:comment>Status indicating the invoice has been cancelled and is no longer valid.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>The current state of a record in the system, including availability and lifecycle stage.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>Status indicating the record has been deleted and is in the trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>Status indicating the record is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>Status indicating the record is in draft form and not yet finalized.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Configuration and Property Classes    -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>A property or characteristic of a field that can be modified by administrators.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistValue">
    <rdfs:label>Picklist Value</rdfs:label>
    <rdfs:comment>An individual option within a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Formula">
    <rdfs:label>Formula</rdfs:label>
    <rdfs:comment>A mathematical or logical expression used to compute field values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupConfiguration">
    <rdfs:label>Lookup Configuration</rdfs:label>
    <rdfs:comment>Configuration settings that define how a lookup field references another module or entity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>A functional component or section of the system that manages specific types of records.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Tracking and Audit Classes            -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#AuditTrail">
    <rdfs:label>Audit Trail</rdfs:label>
    <rdfs:comment>A chronological record of system activities and changes for compliance and tracking purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HistoryTracking">
    <rdfs:label>History Tracking</rdfs:label>
    <rdfs:comment>A mechanism for recording changes to field values over time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Activity">
    <rdfs:label>Activity</rdfs:label>
    <rdfs:comment>An action or event associated with a record, such as calls, meetings, or tasks.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Integration and Communication Classes -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Webhook">
    <rdfs:label>Webhook</rdfs:label>
    <rdfs:comment>An automated message sent from the system to external applications when specific events occur.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>A component that extracts and processes information from email messages.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties - Relationships     -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasOwner">
    <rdfs:label>has owner</rdfs:label>
    <rdfs:comment>Relates an invoice to the user who owns or is responsible for it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#ownedBy">
    <rdfs:label>owned by</rdfs:label>
    <rdfs:comment>Inverse of hasOwner - indicates which user owns a record.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasOwner"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#relatedToSalesOrder">
    <rdfs:label>related to sales order</rdfs:label>
    <rdfs:comment>Links an invoice to its originating sales order.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#SalesOrder"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#generatesInvoice">
    <rdfs:label>generates invoice</rdfs:label>
    <rdfs:comment>Indicates that a sales order generates an invoice.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#SalesOrder"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Invoice"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#relatedToSalesOrder"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasAccount">
    <rdfs:label>has account</rdfs:label>
    <rdfs:comment>Associates an invoice with a customer account.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Account"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#accountFor">
    <rdfs:label>account for</rdfs:label>
    <rdfs:comment>Inverse relationship indicating which invoices belong to an account.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Account"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Invoice"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasAccount"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasContact">
    <rdfs:label>has contact</rdfs:label>
    <rdfs:comment>Associates an invoice with a specific contact person.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#contactFor">
    <rdfs:label>contact for</rdfs:label>
    <rdfs:comment>Indicates which invoices a contact is associated with.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Invoice"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasContact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasField">
    <rdfs:label>has field</rdfs:label>
    <rdfs:comment>Indicates that an entity contains a specific field.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Invoice"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#fieldOf">
    <rdfs:label>field of</rdfs:label>
