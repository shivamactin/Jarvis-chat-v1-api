<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Sales Order Management System Ontology</rdfs:label>
    <rdfs:comment>Comprehensive ontology modeling sales order fields, business entities, relationships, and operational workflows in a CRM system. This ontology captures the complete semantic structure of sales order management including customer relationships, product ordering, financial transactions, approval workflows, and organizational hierarchies.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Business Entity Classes -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#BusinessEntity">
    <rdfs:label>Business Entity</rdfs:label>
    <rdfs:comment>Abstract superclass representing any entity participating in business operations including organizations, individuals, and business objects.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SalesOrder">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessDocument"/>
    <rdfs:label>Sales Order</rdfs:label>
    <rdfs:comment>A formal document representing a customer's commitment to purchase products or services, containing detailed line items, pricing, terms, and delivery information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Business Document</rdfs:label>
    <rdfs:comment>Formal documents used in business transactions including orders, invoices, quotes, and purchase orders.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Deal">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessOpportunity"/>
    <rdfs:label>Deal</rdfs:label>
    <rdfs:comment>A potential or actual business opportunity representing a sales prospect that may result in revenue generation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BusinessOpportunity">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Business Opportunity</rdfs:label>
    <rdfs:comment>Potential revenue-generating activities including deals, leads, and prospects.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Quote">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessDocument"/>
    <rdfs:label>Quote</rdfs:label>
    <rdfs:comment>A formal price quotation provided to a customer outlining proposed products, services, and pricing terms.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Invoice">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#FinancialDocument"/>
    <rdfs:label>Invoice</rdfs:label>
    <rdfs:comment>A financial document requesting payment for goods or services delivered, derived from sales orders.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FinancialDocument">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessDocument"/>
    <rdfs:label>Financial Document</rdfs:label>
    <rdfs:comment>Documents related to financial transactions including invoices, payment records, and financial statements.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PurchaseOrder">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessDocument"/>
    <rdfs:label>Purchase Order</rdfs:label>
    <rdfs:comment>A commercial document issued by a buyer to a seller indicating types, quantities, and agreed prices for products or services.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Organizational Entity Classes -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Organization">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Organization</rdfs:label>
    <rdfs:comment>A structured group of people with a collective goal, including companies, institutions, and business entities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Organization"/>
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>A customer organization or company with which business relationships are maintained.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Vendor">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Organization"/>
    <rdfs:label>Vendor</rdfs:label>
    <rdfs:comment>A supplier or partner organization that provides goods, services, or acts as an intermediary in business transactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Partner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Vendor"/>
    <rdfs:label>Partner</rdfs:label>
    <rdfs:comment>A strategic business partner involved in sales, distribution, or service delivery.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReferralPartner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Partner"/>
    <rdfs:label>Referral Partner</rdfs:label>
    <rdfs:comment>A partner who generates leads and refers business opportunities in exchange for commission or fees.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StrategicPartner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Partner"/>
    <rdfs:label>Strategic Partner</rdfs:label>
    <rdfs:comment>A partner with deep business integration and collaborative relationship for mutual benefit.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ChannelPartner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Partner"/>
    <rdfs:label>Channel Partner</rdfs:label>
    <rdfs:comment>A partner who resells or distributes products and services through established channels.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Dealer">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ChannelPartner"/>
    <rdfs:label>Dealer</rdfs:label>
    <rdfs:comment>An authorized reseller who purchases and sells products to end customers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Consultant">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Partner"/>
    <rdfs:label>Consultant</rdfs:label>
    <rdfs:comment>A professional advisor who provides expert guidance and recommendations to clients.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Person and Contact Classes -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Person">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#BusinessEntity"/>
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>An individual human being participating in business activities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>An individual associated with an account or organization who participates in business communications and transactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A system user with access credentials and assigned roles for performing business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SalesOrderOwner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Sales Order Owner</rdfs:label>
    <rdfs:comment>The user responsible for managing and overseeing a specific sales order.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Product and Service Classes -->
<!-- ===================================== -->

