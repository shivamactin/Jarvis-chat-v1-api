</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ImplementationService">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ServiceProduct"/>
    <rdfs:label>Implementation Service</rdfs:label>
