    <rdfs:label>Invoiced Item</rdfs:label>
    <rdfs:comment>A line item included in an invoice for billing purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HardwareProduct">
