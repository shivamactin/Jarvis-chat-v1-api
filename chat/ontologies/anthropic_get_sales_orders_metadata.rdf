</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ServiceProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Service Product</rdfs:label>
    <rdfs:comment>Intangible services offered including implementation, maintenance, and support.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataService">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ServiceProduct"/>
    <rdfs:label>Data Service</rdfs:label>
