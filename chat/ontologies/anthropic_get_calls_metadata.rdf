<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Call Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling call management, CRM field metadata, user interactions, telephony operations, and business process workflows. This ontology captures entities, relationships, and constraints derived from call tracking system metadata including fields, profiles, permissions, data types, and operational characteristics.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>Represents a data field in the CRM system that captures specific information about calls or related entities. Fields have properties such as data type, visibility, editability, and business rules.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field that is automatically managed by the system, typically read-only and used for tracking system-level information like creation time, modification time, or record identifiers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UserDefinedField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>User Defined Field</rdfs:label>
    <rdfs:comment>A field that can be customized by users or administrators to capture domain-specific information beyond standard system fields.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must be populated with a value before a record can be saved or submitted.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OptionalField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Optional Field</rdfs:label>
    <rdfs:comment>A field that may be left empty without preventing record creation or modification.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Call">
    <rdfs:label>Call</rdfs:label>
    <rdfs:comment>Represents a telephone call interaction in the CRM system, including inbound, outbound, and missed calls. Calls are associated with contacts, accounts, and other business entities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#InboundCall">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:label>Inbound Call</rdfs:label>
    <rdfs:comment>A call received by the organization from an external party such as a customer, prospect, or partner.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OutboundCall">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:label>Outbound Call</rdfs:label>
    <rdfs:comment>A call initiated by the organization to an external party for purposes such as sales, support, or follow-up.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MissedCall">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:label>Missed Call</rdfs:label>
    <rdfs:comment>A call that was not answered or connected, requiring follow-up action.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ScheduledCall">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#OutboundCall"/>
    <rdfs:label>Scheduled Call</rdfs:label>
    <rdfs:comment>An outbound call that has been planned for a specific date and time in the future.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CompletedCall">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:label>Completed Call</rdfs:label>
    <rdfs:comment>A call that has been successfully conducted and concluded.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>Represents an individual person in the CRM system, typically a customer, prospect, or business contact with whom the organization communicates.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>Represents a person who uses the CRM system, with specific roles, permissions, and responsibilities for managing data and conducting business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CallOwner">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Call Owner</rdfs:label>
    <rdfs:comment>A user who is responsible for managing and following up on a specific call record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Administrator">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Administrator</rdfs:label>
    <rdfs:comment>A user with elevated privileges to configure system settings, manage users, and customize the CRM application.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#StandardUser">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#User"/>
    <rdfs:label>Standard User</rdfs:label>
    <rdfs:comment>A regular user with standard permissions to create, read, update, and delete records within their assigned scope.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>Defines a set of permissions and access rights that determine what actions users can perform and what data they can access in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>Represents a specific access right or capability granted to a user or profile, such as read, write, create, or delete operations on particular data types.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Permission</rdfs:label>
    <rdfs:comment>Permission to view and read data without the ability to modify it.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Write Permission</rdfs:label>
    <rdfs:comment>Permission to modify existing data records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Combined permission to both view and modify data records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>Specifies the type of data that can be stored in a field, such as text, number, date, boolean, or lookup reference.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Text Data Type</rdfs:label>
    <rdfs:comment>A data type for storing alphanumeric text strings of varying lengths.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>A data type for storing date and time values with precision to seconds or milliseconds.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#IntegerDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Integer Data Type</rdfs:label>
    <rdfs:comment>A data type for storing whole numbers without decimal places.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>A data type for storing true/false or yes/no values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Picklist Data Type</rdfs:label>
    <rdfs:comment>A data type that restricts values to a predefined list of options from which users can select.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Lookup Data Type</rdfs:label>
    <rdfs:comment>A data type that creates a reference relationship to another entity or module in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>A specialized lookup that references user records to establish ownership or responsibility for a record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistValue">
    <rdfs:label>Picklist Value</rdfs:label>
    <rdfs:comment>Represents an individual option within a picklist field, with display value, actual value, and sequence information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>Defines the visual arrangement and grouping of fields on a form or page in the CRM interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Module">
    <rdfs:label>Module</rdfs:label>
    <rdfs:comment>Represents a functional area or entity type in the CRM system, such as Calls, Contacts, Accounts, or Leads.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CallsModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>Calls Module</rdfs:label>
    <rdfs:comment>The module specifically dedicated to managing call records and telephony interactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ContactsModule">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Module"/>
    <rdfs:label>Contacts Module</rdfs:label>
    <rdfs:comment>The module for managing individual contact records and personal information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Operation">
    <rdfs:label>Operation</rdfs:label>
    <rdfs:comment>Represents an action that can be performed on data, such as create, read, update, or delete.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Create Operation</rdfs:label>
    <rdfs:comment>The action of creating a new record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Update Operation</rdfs:label>
    <rdfs:comment>The action of modifying an existing record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Web Operation</rdfs:label>
    <rdfs:comment>An operation performed through the web-based user interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#APIOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>API Operation</rdfs:label>
    <rdfs:comment>An operation performed programmatically through the application programming interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>Defines the context in which a field is displayed or accessible, such as in detail view, edit view, or quick create forms.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CallPurpose">
    <rdfs:label>Call Purpose</rdfs:label>
    <rdfs:comment>Categorizes the business objective or reason for a call, such as prospecting, negotiation, demo, or support.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProspectingPurpose">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallPurpose"/>
    <rdfs:label>Prospecting Purpose</rdfs:label>
    <rdfs:comment>A call made to identify and qualify potential new customers or business opportunities.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NegotiationPurpose">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallPurpose"/>
    <rdfs:label>Negotiation Purpose</rdfs:label>
    <rdfs:comment>A call focused on discussing terms, pricing, or contract details with a prospect or customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DemoPurpose">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallPurpose"/>
    <rdfs:label>Demo Purpose</rdfs:label>
    <rdfs:comment>A call conducted to demonstrate product features, capabilities, or solutions to a prospect.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SupportPurpose">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallPurpose"/>
    <rdfs:label>Support Purpose</rdfs:label>
    <rdfs:comment>A call made to provide customer service, technical assistance, or problem resolution.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CallResult">
    <rdfs:label>Call Result</rdfs:label>
    <rdfs:comment>Captures the outcome or disposition of a call, indicating whether the contact was interested, requested information, or was unavailable.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#InterestedResult">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallResult"/>
    <rdfs:label>Interested Result</rdfs:label>
    <rdfs:comment>Indicates that the contact expressed interest in the product, service, or proposal discussed.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NotInterestedResult">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallResult"/>
    <rdfs:label>Not Interested Result</rdfs:label>
    <rdfs:comment>Indicates that the contact declined interest or is not a viable prospect at this time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#NoResponseResult">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallResult"/>
    <rdfs:label>No Response Result</rdfs:label>
    <rdfs:comment>Indicates that the call was not answered or the contact was busy and unavailable.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CallStatus">
    <rdfs:label>Call Status</rdfs:label>
    <rdfs:comment>Represents the current state of a call in its lifecycle, such as scheduled, completed, overdue, or cancelled.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ScheduledStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallStatus"/>
    <rdfs:label>Scheduled Status</rdfs:label>
    <rdfs:comment>Indicates that a call has been planned for a future date and time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CompletedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallStatus"/>
    <rdfs:label>Completed Status</rdfs:label>
    <rdfs:comment>Indicates that a call has been successfully conducted and finished.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OverdueStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallStatus"/>
    <rdfs:label>Overdue Status</rdfs:label>
    <rdfs:comment>Indicates that a scheduled call was not completed by its planned time and requires attention.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CancelledStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CallStatus"/>
    <rdfs:label>Cancelled Status</rdfs:label>
    <rdfs:comment>Indicates that a scheduled call has been cancelled and will not take place.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>Indicates the lifecycle state of a record in the system, such as available, draft, or trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableRecord">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Record</rdfs:label>
    <rdfs:comment>A record that is active and accessible for normal business operations.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftRecord">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Record</rdfs:label>
    <rdfs:comment>A record that is being created or edited but has not yet been finalized or published.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashRecord">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Record</rdfs:label>
    <rdfs:comment>A record that has been deleted and moved to the recycle bin, pending permanent removal.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Reminder">
    <rdfs:label>Reminder</rdfs:label>
    <rdfs:comment>A notification or alert set to remind users about an upcoming call or task at a specified time before the event.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CTIEntry">
    <rdfs:label>CTI Entry</rdfs:label>
    <rdfs:comment>Represents a call record created through Computer Telephony Integration, automatically logged from phone system interactions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#VoiceRecording">
    <rdfs:label>Voice Recording</rdfs:label>
    <rdfs:comment>An audio recording of a call conversation, stored for quality assurance, training, or compliance purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailParser">
    <rdfs:label>Email Parser</rdfs:label>
    <rdfs:comment>A system component that extracts structured data from email messages to automatically create or update CRM records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Webhook">
    <rdfs:label>Webhook</rdfs:label>
    <rdfs:comment>An automated mechanism for sending real-time data updates to external systems when specific events occur in the CRM.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Blueprint">
    <rdfs:label>Blueprint</rdfs:label>
    <rdfs:comment>A workflow automation tool that defines stages, transitions, and business rules for guiding records through standardized processes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Tag">
    <rdfs:label>Tag</rdfs:label>
    <rdfs:comment>A keyword or label applied to records for categorization, filtering, and organization purposes.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#RelatedEntity">
    <rdfs:label>Related Entity</rdfs:label>
    <rdfs:comment>An entity in the CRM system that has a business relationship or association with a call, such as an account, deal, or case.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RelatedEntity"/>
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>Represents a company or organization with which the business has a relationship.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Deal">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RelatedEntity"/>
    <rdfs:label>Deal</rdfs:label>
    <rdfs:comment>Represents a sales opportunity or potential transaction being pursued with a prospect or customer.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Lead">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RelatedEntity"/>
    <rdfs:label>Lead</rdfs:label>
    <rdfs:comment>Represents a potential customer who has shown interest but has not yet been qualified or converted.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomizableProperty">
    <rdfs:label>Customizable Property</rdfs:label>
    <rdfs:comment>A characteristic of a field that can be modified by administrators, such as mandatory status, tooltip text, or picklist values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HistoryTracking">
    <rdfs:label>History Tracking</rdfs:label>
    <rdfs:comment>A feature that records changes to field values over time, creating an audit trail of modifications.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MassUpdate">
    <rdfs:label>Mass Update</rdfs:label>
    <rdfs:comment>An operation that allows bulk modification of field values across multiple records simultaneously.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties (Relationships)     -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasOwner">
    <rdfs:label>has owner</rdfs:label>
    <rdfs:comment>Relates a call to the user who is responsible for managing it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:range rdf:resource="http://example.org/ontology#CallOwner"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#owns">
    <rdfs:label>owns</rdfs:label>
    <rdfs:comment>Relates a user to the calls they are responsible for managing.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#CallOwner"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Call"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasOwner"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#relatedToContact">
    <rdfs:label>related to contact</rdfs:label>
    <rdfs:comment>Associates a call with the contact person who participated in or is the subject of the call.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#participatedInCall">
    <rdfs:label>participated in call</rdfs:label>
    <rdfs:comment>Relates a contact to calls in which they were involved.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Call"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#relatedToContact"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#relatedToEntity">
    <rdfs:label>related to entity</rdfs:label>
    <rdfs:comment>Associates a call with a related business entity such as an account, deal, or lead.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Call"/>
    <rdfs:range rdf:resource="http://example.org/ontology#RelatedEntity"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasAssociatedCall">
    <rdfs:label>has associated call</rdfs:label>
    <rdfs:comment>Relates a business entity to calls that reference or involve it.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#RelatedEntity"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Call"/>
