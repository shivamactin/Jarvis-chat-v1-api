<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:ex="http://example.org/ontology#">

<owl:Ontology rdf:about="http://example.org/ontology#">
    <rdfs:label>Contact Management System Ontology</rdfs:label>
    <rdfs:comment>A comprehensive ontology modeling contact records, field metadata, user profiles, permissions, data types, operations, and their complex interrelationships within a CRM system context.</rdfs:comment>
    <owl:versionInfo>1.0</owl:versionInfo>
</owl:Ontology>

<!-- ===================================== -->
<!-- Core Entity Classes                   -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Contact">
    <rdfs:label>Contact</rdfs:label>
    <rdfs:comment>A person or entity record within the CRM system containing personal, professional, and interaction information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Field">
    <rdfs:label>Field</rdfs:label>
    <rdfs:comment>A metadata definition describing a data field within the contact management system, including its properties, constraints, and behavior.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Account">
    <rdfs:label>Account</rdfs:label>
    <rdfs:comment>An organizational entity or company associated with contacts in the CRM system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#User">
    <rdfs:label>User</rdfs:label>
    <rdfs:comment>A system user who can create, modify, and manage records within the CRM.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Profile">
    <rdfs:label>Profile</rdfs:label>
    <rdfs:comment>A user profile defining access permissions and capabilities within the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Layout">
    <rdfs:label>Layout</rdfs:label>
    <rdfs:comment>A visual arrangement and organization of fields for data entry and display.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Campaign">
    <rdfs:label>Campaign</rdfs:label>
    <rdfs:comment>A marketing or outreach campaign that can be associated with contacts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Territory">
    <rdfs:label>Territory</rdfs:label>
    <rdfs:comment>A geographical or organizational territory for contact assignment and management.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Field Type Hierarchy                  -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#SystemField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>System Field</rdfs:label>
    <rdfs:comment>A field automatically managed by the system with restricted user modification.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CustomField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Custom Field</rdfs:label>
    <rdfs:comment>A user-defined field created to capture specific business requirements.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#SystemField"/>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MandatoryField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Mandatory Field</rdfs:label>
    <rdfs:comment>A field that must contain a value before record submission.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Read Only Field</rdfs:label>
    <rdfs:comment>A field that cannot be modified by users through standard interfaces.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UniqueField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Unique Field</rdfs:label>
    <rdfs:comment>A field whose value must be unique across all records.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SearchableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Searchable Field</rdfs:label>
    <rdfs:comment>A field that can be used in search queries and filters.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#FilterableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Filterable Field</rdfs:label>
    <rdfs:comment>A field that can be used to filter record lists.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SortableField">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:label>Sortable Field</rdfs:label>
    <rdfs:comment>A field by which records can be sorted.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Data Type Classes                     -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#DataType">
    <rdfs:label>Data Type</rdfs:label>
    <rdfs:comment>The type of data that a field can contain.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Text Data Type</rdfs:label>
    <rdfs:comment>A data type for storing textual information.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EmailDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Email Data Type</rdfs:label>
    <rdfs:comment>A data type for storing email addresses with validation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PhoneDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Phone Data Type</rdfs:label>
    <rdfs:comment>A data type for storing phone numbers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DateTimeDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>DateTime Data Type</rdfs:label>
    <rdfs:comment>A data type for storing date and time values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BooleanDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Boolean Data Type</rdfs:label>
    <rdfs:comment>A data type for storing true/false values.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#IntegerDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Integer Data Type</rdfs:label>
    <rdfs:comment>A data type for storing whole numbers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DoubleDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Double Data Type</rdfs:label>
    <rdfs:comment>A data type for storing decimal numbers with precision.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PicklistDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Picklist Data Type</rdfs:label>
    <rdfs:comment>A data type for selecting from predefined options.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#LookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Lookup Data Type</rdfs:label>
    <rdfs:comment>A data type for referencing records in other modules.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#OwnerLookupDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LookupDataType"/>
    <rdfs:label>Owner Lookup Data Type</rdfs:label>
    <rdfs:comment>A specialized lookup for referencing record owners.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TextAreaDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#TextDataType"/>
    <rdfs:label>Text Area Data Type</rdfs:label>
    <rdfs:comment>A data type for storing large blocks of text.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebsiteDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Website Data Type</rdfs:label>
    <rdfs:comment>A data type for storing URL addresses.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ProfileImageDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#DataType"/>
    <rdfs:label>Profile Image Data Type</rdfs:label>
    <rdfs:comment>A data type for storing image references.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#BigIntDataType">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#IntegerDataType"/>
    <rdfs:label>Big Integer Data Type</rdfs:label>
    <rdfs:comment>A data type for storing very large integer values.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Operation and Permission Classes      -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Operation">
    <rdfs:label>Operation</rdfs:label>
    <rdfs:comment>An action that can be performed on a field or record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Create Operation</rdfs:label>
    <rdfs:comment>The operation of creating a new record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Operation"/>
    <rdfs:label>Update Operation</rdfs:label>
    <rdfs:comment>The operation of modifying an existing record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>Web Create Operation</rdfs:label>
    <rdfs:comment>Creating a record through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiCreateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateOperation"/>
    <rdfs:label>API Create Operation</rdfs:label>
    <rdfs:comment>Creating a record through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>Web Update Operation</rdfs:label>
    <rdfs:comment>Updating a record through the web interface.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ApiUpdateOperation">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#UpdateOperation"/>
    <rdfs:label>API Update Operation</rdfs:label>
    <rdfs:comment>Updating a record through the API.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Permission">
    <rdfs:label>Permission</rdfs:label>
    <rdfs:comment>An authorization level for accessing or modifying data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadWritePermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Write Permission</rdfs:label>
    <rdfs:comment>Permission to both view and modify data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReadOnlyPermission">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Permission"/>
    <rdfs:label>Read Only Permission</rdfs:label>
    <rdfs:comment>Permission to view but not modify data.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- View and Display Classes              -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#ViewType">
    <rdfs:label>View Type</rdfs:label>
    <rdfs:comment>A context in which a field can be displayed or edited.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DetailView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Detail View</rdfs:label>
    <rdfs:comment>A view showing complete record details.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EditView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Edit View</rdfs:label>
    <rdfs:comment>A view for modifying record data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#CreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#ViewType"/>
    <rdfs:label>Create View</rdfs:label>
    <rdfs:comment>A view for entering new record data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#QuickCreateView">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#CreateView"/>
    <rdfs:label>Quick Create View</rdfs:label>
    <rdfs:comment>A simplified view for rapid record creation.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Address and Location Classes          -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Address">
    <rdfs:label>Address</rdfs:label>
    <rdfs:comment>A physical or mailing address with structured components.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#MailingAddress">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Address"/>
    <rdfs:label>Mailing Address</rdfs:label>
    <rdfs:comment>The address used for postal correspondence.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Picklist and Value Classes            -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#PicklistValue">
    <rdfs:label>Picklist Value</rdfs:label>
    <rdfs:comment>A selectable option within a picklist field.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UsedPicklistValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PicklistValue"/>
    <rdfs:label>Used Picklist Value</rdfs:label>
    <rdfs:comment>A picklist value currently in active use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#UnusedPicklistValue">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#PicklistValue"/>
    <rdfs:label>Unused Picklist Value</rdfs:label>
    <rdfs:comment>A picklist value available but not currently used.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://example.org/ontology#UsedPicklistValue"/>
</owl:Class>

<!-- ===================================== -->
<!-- Source and Integration Classes        -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#LeadSource">
    <rdfs:label>Lead Source</rdfs:label>
    <rdfs:comment>The origin or channel through which a contact was acquired.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AdvertisementSource">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LeadSource"/>
    <rdfs:label>Advertisement Source</rdfs:label>
    <rdfs:comment>A contact acquired through advertising campaigns.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TradeShowSource">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LeadSource"/>
    <rdfs:label>Trade Show Source</rdfs:label>
    <rdfs:comment>A contact acquired at trade shows or exhibitions.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#PartnerSource">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LeadSource"/>
    <rdfs:label>Partner Source</rdfs:label>
    <rdfs:comment>A contact referred by business partners or dealers.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebSource">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LeadSource"/>
    <rdfs:label>Web Source</rdfs:label>
    <rdfs:comment>A contact acquired through web-based channels.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ReferralSource">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#LeadSource"/>
    <rdfs:label>Referral Source</rdfs:label>
    <rdfs:comment>A contact acquired through referrals.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Activity and Tracking Classes         -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Activity">
    <rdfs:label>Activity</rdfs:label>
    <rdfs:comment>An interaction or event associated with a contact.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#WebVisit">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Activity"/>
    <rdfs:label>Web Visit</rdfs:label>
    <rdfs:comment>A visit to a website tracked for the contact.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#Chat">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Activity"/>
    <rdfs:label>Chat</rdfs:label>
    <rdfs:comment>A chat conversation with the contact.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Product and Service Classes           -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#Product">
    <rdfs:label>Product</rdfs:label>
    <rdfs:comment>A product or service offering that can be associated with contacts.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#HardwareProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Hardware Product</rdfs:label>
    <rdfs:comment>A physical hardware product.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#SoftwareProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Software Product</rdfs:label>
    <rdfs:comment>A software or digital product.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#ServiceProduct">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#Product"/>
    <rdfs:label>Service Product</rdfs:label>
    <rdfs:comment>A service offering.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Record State Classes                  -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#RecordStatus">
    <rdfs:label>Record Status</rdfs:label>
    <rdfs:comment>The current state or status of a record in the system.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#AvailableStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Available Status</rdfs:label>
    <rdfs:comment>A record that is active and available for use.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#TrashStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Trash Status</rdfs:label>
    <rdfs:comment>A record that has been deleted and is in the trash.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DraftStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#RecordStatus"/>
    <rdfs:label>Draft Status</rdfs:label>
    <rdfs:comment>A record that is being created but not yet finalized.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Enrichment Classes                    -->
<!-- ===================================== -->

<owl:Class rdf:about="http://example.org/ontology#EnrichmentStatus">
    <rdfs:label>Enrichment Status</rdfs:label>
    <rdfs:comment>The status of data enrichment for a contact record.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#EnrichedStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#EnrichmentStatus"/>
    <rdfs:label>Enriched Status</rdfs:label>
    <rdfs:comment>A record that has been successfully enriched with additional data.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="http://example.org/ontology#DataNotFoundStatus">
    <rdfs:subClassOf rdf:resource="http://example.org/ontology#EnrichmentStatus"/>
    <rdfs:label>Data Not Found Status</rdfs:label>
    <rdfs:comment>A record for which enrichment data could not be found.</rdfs:comment>
</owl:Class>

<!-- ===================================== -->
<!-- Object Properties - Core Relationships -->
<!-- ===================================== -->

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasField">
    <rdfs:label>has field</rdfs:label>
    <rdfs:comment>Relates a contact to its constituent fields.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Field"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#isFieldOf">
    <rdfs:label>is field of</rdfs:label>
    <rdfs:comment>Inverse of hasField - relates a field to its parent contact.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Field"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#hasField"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#belongsToAccount">
    <rdfs:label>belongs to account</rdfs:label>
    <rdfs:comment>Associates a contact with an organizational account.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Account"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#hasContact">
    <rdfs:label>has contact</rdfs:label>
    <rdfs:comment>Inverse relationship - an account has contacts.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Account"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#belongsToAccount"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#ownedBy">
    <rdfs:label>owned by</rdfs:label>
    <rdfs:comment>Indicates the user who owns a contact record.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#owns">
    <rdfs:label>owns</rdfs:label>
    <rdfs:comment>Inverse of ownedBy - a user owns contacts.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#User"/>
    <rdfs:range rdf:resource="http://example.org/ontology#Contact"/>
    <owl:inverseOf rdf:resource="http://example.org/ontology#ownedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#createdBy">
    <rdfs:label>created by</rdfs:label>
    <rdfs:comment>Indicates the user who created a record.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Contact"/>
    <rdfs:range rdf:resource="http://example.org/ontology#User"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://example.org/ontology#created">
    <rdfs:label>created</rdfs:label>
