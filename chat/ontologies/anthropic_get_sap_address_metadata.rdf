<rdf:Description rdf:about="http://example.org/ontology#containsField">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/ontology#partOfModule">
